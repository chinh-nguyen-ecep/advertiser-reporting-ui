	<queryString>
		<![CDATA[SELECT
((TIMESTAMP WITH Time Zone 'epoch' + a.epoch_hour* INTERVAL '1 second') AT TIME ZONE $P{p_timeZone})::date as full_date,
EXTRACT(HOUR FROM (TIMESTAMP WITH Time Zone 'epoch' + a.epoch_hour * INTERVAL '1 second') AT TIME ZONE $P{p_timeZone})::int as hour24_of_day,
a.ave_won_price,
a.ave_bid_price,
a.paid_amount,
a.wins
FROM
(
SELECT epoch_hour, avg(ave_won_price) AS ave_won_price ,avg(ave_bid_price) AS ave_bid_price ,SUM(paid_amount) AS paid_amount,SUM(wins) as wins  FROM rtb.vw_agg_exchange_cost_analysis_by_hour
WHERE epoch_hour BETWEEN $P{p_start_epoch}::int AND  $P{p_end_epoch}::int
AND (exchange=$P{p_exchange} OR 'All Exchanges'=$P{p_exchange})
GROUP BY epoch_hour
ORDER BY epoch_hour
) a]]>
	</queryString>
