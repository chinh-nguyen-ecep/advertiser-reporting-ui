    <actions> 
      <condition><![CDATA[p_mode=='date']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Get date list</action-type>
        <action-inputs> 
          <p_start_date type="string"/>  
          <p_end_date type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="q_date_list"/> 
        </action-outputs>
        <component-definition> 
          <jndi><![CDATA[dw3]]></jndi>  
          <query><![CDATA[SELECT 
 full_date
FROM 
  refer.date_dim
WHERE 
  full_date BETWEEN '{p_start_date}' AND '{p_end_date}' GROUP BY full_date ORDER BY full_date ASC;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Convert Parameter to OLAP script</action-type>
      <action-inputs> 
        <p_measures type="string"/>  
        <device type="string-list"/>  
        <q_date_list type="result-set"/>  
        <p_network type="string"/>  
        <p_start_month type="string"/>  
        <p_end_month type="string"/>  
        <q_calendar_year_month type="result-set"/>  
        <p_dimension_mode type="string"/> 
      </action-inputs>
      <action-outputs> 
        <t_measures type="string"/>  
        <t_device type="string"/>  
        <t_date_list type="string"/>  
        <t_month_list type="string"/>  
        <t_network type="string"/>  
        <t_agg type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var t_measures;
var t_device;
var t_date_list;
var t_month_list;
var t_network;
var t_agg;

//set default value
t_measures='';
t_device='';
t_date_list='';
t_month_list='';
t_network='';
t_agg='';
//convert measures
if(p_measures instanceof Array){
	for(var i=0;i<p_measures.length;i++){
		if(i>0){
		t_measures+=',';
		};
		t_measures+='[Measures].['+p_measures[i]+']';
	};
}else{
	t_measures+='[Measures].['+p_measures+']';
};

//convert dimension
var fistTextOfDimension;
if(p_dimension_mode=='Device'){
	fistTextOfDimension='Device type';
	t_agg='Daily unfilled stats by device';
}else if(p_dimension_mode=='Partner'){
	fistTextOfDimension='Partner';
	t_agg='Daily unfilled stats';
}else if(p_dimension_mode=='Portal'){
	fistTextOfDimension='Portal';
	t_agg='Daily unfilled stats';
}else if(p_dimension_mode=='Group'){
	fistTextOfDimension='Parent';
	t_agg='Daily unfilled stats';
}else if(p_dimension_mode=='Metro'){
	fistTextOfDimension='Metro';
	t_agg='Daily unfilled stats by DMA';
}else if(p_dimension_mode=='Category'){
	fistTextOfDimension='Category';
	t_agg='Daily unfilled stats by Category';
};
if(device instanceof Array){
	for(var i=0;i<device.length;i++){
		if(i>0){
		t_device+=',';
		};
		t_device+='['+fistTextOfDimension+'].['+device[i]+']';
	};
}else{
	t_device+='['+fistTextOfDimension+'].['+device+']';
};

//convert network
if(p_network instanceof Array){
	for(var i=0;i<p_network.length;i++){
		if(i>0){
		t_network+=',';
		};
		t_network+='[Network].['+p_network[i]+']';
	};
}else{
	t_network+='[Network].['+p_network+']';
};

//convert date list
var countDateList=q_date_list.getRowCount();
for(var i=0;i<countDateList;i++){
		if(i>0){
			t_date_list+=',';
		};
		t_date_list+='[Date].['+q_date_list.getValueAt(i,0)+']';		
	};

//convert month list
var minRage;
var maxRage;
if(p_start_month<p_end_month){
	minRage=p_start_month;
	maxRage=p_end_month;
}else{
	minRage=p_end_month;
	maxRage=p_start_month;
};

for(var i=maxRage;i>=minRage;i--){
		if(i<maxRage){
			t_month_list+=',';
		};
		t_month_list+='[Year month].['+q_calendar_year_month.getValueAt(i,1)+']';
				
	};]]></script> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Get user choose mode</action-type>
      <action-inputs> 
        <p_measures type="string"/>  
        <device type="string-list"/> 
      </action-inputs>
      <action-outputs> 
        <v_user_choose_mode type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var v_user_choose_mode;
var measures;
var dimension;

measures=typeof(p_measures);
dimension=typeof(device);

if( measures == 'string' && dimension == 'object'){
	v_user_choose_mode='multiDimensions';
}else if( measures == 'object' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'string' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'object' && dimension == 'object'){
	v_user_choose_mode = 'multiDeviceMeasures';
}]]></script> 
      </component-definition> 
    </action-definition>
  
    <actions> 
      <condition><![CDATA[v_user_choose_mode=='multiDimensions']]></condition>  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Make OLAP query</action-type>
        <action-inputs> 
          <queryOLAP type="string"/>  
          <v_chart_title type="string"/>  
          <t_measures type="string"/>  
          <t_device type="string"/>  
          <t_date_list type="string"/>  
          <t_month_list type="string"/>  
          <t_network type="string"/>  
          <p_dimension_mode type="string"/>  
          <p_mode type="string"/>  
          <p_network type="string"/>  
          <p_measures type="string"/>  
          <device type="string-list"/>  
          <t_agg type="string"/> 
        </action-inputs>
        <action-outputs> 
          <queryOLAP type="string"/>  
          <v_chart_sub_title type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[v_chart_sub_title=p_measures+' by '+p_dimension_mode+' on '+ p_network;
if(p_dimension_mode=='Device'){
	if(p_mode=='date'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_date_list;
	queryOLAP+='} ON ROWS from [Daily unfilled stats by device] where Crossjoin({[IsActive].[true]}, {'+t_network+'})';
}else if(p_mode=='month'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_month_list;
	queryOLAP+='} ON ROWS from [Daily unfilled stats by device] where Crossjoin({[IsActive].[true]}, {'+t_network+'})';
};
	
}else{
if(p_mode=='date'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_date_list;
	queryOLAP+='} ON ROWS from ['+t_agg+'] where Crossjoin({[IsActive].[true]}, {'+t_network+'})';
}else if(p_mode=='month'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_month_list;
	queryOLAP+='} ON ROWS from ['+t_agg+'] where Crossjoin({[IsActive].[true]},{'+t_network+'})';
};
}]]></script> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[v_user_choose_mode=='multiMeasures']]></condition>  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Make OLAP query</action-type>
