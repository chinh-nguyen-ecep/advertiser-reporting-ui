<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subreport_partner_detail" pageWidth="555" pageHeight="802" columnWidth="535" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<parameter name="eastern_date_sk" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="parent1_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     a.eastern_date_sk,
     a.partner_id,
     a.partner_name,
     a.partner_description || ' (' || a.partner_id::text || ')' as unique_partner_name,
     a.unique_view_count,
     a.unique_visit_count,
     a.page_view_count
FROM
     dw.daily_agg_partner_act_all a
WHERE
     a.eastern_date_sk = $P{eastern_date_sk}::int
 AND a.parent1_id = $P{parent1_id}::int
 AND a.organic = true
 AND a.is_active = true
ORDER BY unique_partner_name]]>
	</queryString>
	<field name="eastern_date_sk" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="partner_id" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="partner_name" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="unique_partner_name" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="unique_view_count" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="unique_visit_count" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="page_view_count" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<background>
		<band/>
	</background>
	<title>
		<band height="80">
			<line>
				<reportElement positionType="FixRelativeToBottom" x="0" y="50" width="555" height="1"/>
			</line>
			<staticText>
				<reportElement x="0" y="20" width="200" height="20"/>
				<textElement>
					<font fontName="Arial" size="14" isBold="true"/>
				</textElement>
				<text><![CDATA[Partner Detail:]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="60" width="200" height="20"/>
				<textElement>
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[Partner Name]]></text>
			</staticText>
			<staticText>
				<reportElement x="200" y="60" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[uniques]]></text>
			</staticText>
			<staticText>
				<reportElement x="280" y="60" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[page views]]></text>
			</staticText>
			<staticText>
				<reportElement x="360" y="60" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[pg/unique]]></text>
			</staticText>
			<staticText>
				<reportElement x="440" y="60" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[visits]]></text>
			</staticText>
		</band>
	</title>
	<detail>
		<band height="20">
			<textField>
				<reportElement x="0" y="0" width="200" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{unique_partner_name}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="200" y="0" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{unique_view_count}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="280" y="0" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{page_view_count}]]></textFieldExpression>
			</textField>
			<textField pattern="###0.00 %">
				<reportElement x="360" y="0" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Double"><![CDATA[new Double(  ($F{page_view_count}.doubleValue()) /($F{unique_view_count}.doubleValue()) )]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="440" y="0" width="80" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{unique_visit_count}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
