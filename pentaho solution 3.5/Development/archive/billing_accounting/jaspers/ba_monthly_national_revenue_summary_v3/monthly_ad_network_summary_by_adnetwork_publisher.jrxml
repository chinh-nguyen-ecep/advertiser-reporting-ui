	<queryString>
		<![CDATA[SELECT
-100 as advertiser_id
,advertiser_name
,adm_order_id
,flight_id
,flight_metric,
flight_type,
booked_cpm as booked_cpm,
billable_cpm as billable_cpm,
verve_cpm as verve_cpm,
booked_impressions as booked_impressions
,(case when sum(gross_revenue)>0 then sum(v_net_revenue)/sum(billable_revenue) else 0 end) as verve_share,
(case when sum(gross_revenue)>0 then sum(p_net_revenue)/sum(billable_revenue) else 0 end) as publisher_share,
sum(delivered_imps) as delivered_imps,
sum(partner_remaining_imps) as remaining_imps,
sum(partner_billable_imps) as billable_imps,
sum(gross_revenue) as gross_revenue,
sum(p_net_revenue) as p_net_revenue,
sum(v_net_revenue) as v_net_revenue,
sum( billable_revenue) as  billable_revenue
FROM adm.ba_monthly_national_revenue_v3 a
WHERE calendar_year_month=$P{calendar_year_month}
AND delivered_imps>0
AND is_active=true
GROUP BY advertiser_id,advertiser_name,adm_order_id,flight_id,flight_metric,flight_type,booked_cpm,billable_cpm,verve_cpm,booked_impressions
ORDER BY advertiser_id,advertiser_name,adm_order_id,flight_id]]>
	</queryString>
