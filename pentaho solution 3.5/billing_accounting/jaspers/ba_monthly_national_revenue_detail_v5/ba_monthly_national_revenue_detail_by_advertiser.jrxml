	<queryString>
		<![CDATA[SELECT
dfp_advertiser_id as advertiser_id
,dfp_advertiser_name as advertiser_name
,adm_order_id
,flight_id
,upper(flight_metric) as flight_metric
,case network_id
 when 1 then 'Local'
 when 2 then 'National'
 when 3 then 'Remnant'
 when 4 then 'VLM'
 else 'N/A'
 end as flight_network,
booked_rate as booked_rate,
billable_rate as billable_rate,
booked_units as booked_units,
sum(delivered_units) as delivered_units,
sum(partner_remaining_units) as remaining_units,
sum(partner_billable_units) as billable_units,
sum(gross_revenue) as gross_revenue,
sum(p_net_revenue) as p_net_revenue,
sum(v_net_revenue) as v_net_revenue,
sum( billable_revenue) as  billable_revenue
FROM billing.ba_monthly_national_revenue_v5 a
WHERE calendar_year_month=$P{calendar_year_month}
AND delivered_units>0
AND is_active=true
GROUP BY dfp_advertiser_id,dfp_advertiser_name,adm_order_id,flight_id,flight_metric,network_id,booked_rate,billable_rate,booked_units
ORDER BY dfp_advertiser_name,adm_order_id,network_id,flight_id]]>
	</queryString>
