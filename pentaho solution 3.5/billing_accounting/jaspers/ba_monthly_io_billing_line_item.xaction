    <connection_name_2 type="string"> 
      <sources> 
        <request>connection_name_2</request> 
      </sources>  
      <default-value><![CDATA[dw3dw3]]></default-value> 
    </connection_name_2>  
    <List_mode type="result-set"> 
      <sources> 
        <request>List_mode</request> 
      </sources>  
      <default-value type="result-set">
        <row>
          <name>All</name>
          <values>all</values>
        </row>
        <row>
          <name>Month</name>
          <values>month</values>
        </row>
        <row>
          <name>Current Month</name>
          <values>current_month</values>
        </row>
      </default-value>  
      <columns>
        <name type="string"/>
        <values type="string"/>
      </columns>
    </List_mode> 
  </inputs>

