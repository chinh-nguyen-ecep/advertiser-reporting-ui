  <resources> 
    <pageTemplate1> 
      <solution-file> 
        <location>daily_delivery_by_adnetwork_v2.4/tp_ad_pub_prop_channel.html</location>  
        <mime-type>text/plain</mime-type> 
      </solution-file> 
