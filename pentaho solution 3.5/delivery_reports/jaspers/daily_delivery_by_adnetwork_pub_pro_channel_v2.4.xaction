    <report-definition1> 
      <solution-file> 
        <location>daily_delivery_by_adnetwork_v2.4/monthly_delivery_adnetwork_pub_pro_channel.jasper</location>  
        <mime-type>text/plain</mime-type> 
      </solution-file> 
    </report-definition1> 
  </resources>
  
  <actions> 
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
      <action-type>Get max min date data</action-type>
      <action-inputs> 
        <jndi type="string" mapping="connection_name"/> 
      </action-inputs>
      <action-outputs> 
        <query-result type="result-set" mapping="q_max_min_date"/>  
        <max_full_date type="string"/>  
        <min_full_date type="string"/> 
      </action-outputs>
      <component-definition> 
        <live><![CDATA[false]]></live>  
        <query><![CDATA[SELECT MIN (full_date) as min_full_date, MAX (full_date) as max_full_date FROM adsops.daily_agg_delivery_adnetwork_publisher_v3 WHERE is_active=true AND eastern_date_sk>=3653
UNION ALL
SELECT MIN (full_date) as min_full_date, MAX (full_date) as max_full_date FROM adsops.daily_agg_delivery_adnetwork_publisher_v3 WHERE is_active=true AND eastern_date_sk>=3653]]></query> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
      <action-type>Get list of months</action-type>
      <action-inputs> 
        <jndi type="string" mapping="connection_name"/> 
      </action-inputs>
      <action-outputs> 
        <query-result type="result-set" mapping="query_result_list_months"/>  
        <calendar_year_month type="string"/>  
        <month_since_2005 type="string"/> 
      </action-outputs>
      <component-definition> 
        <live><![CDATA[false]]></live>  
        <query><![CDATA[SELECT calendar_year_month,month_since_2005 
FROM adsops.monthly_agg_delivery_adnetwork_publisher_v3 a
GROUP BY calendar_year_month,month_since_2005 
UNION ALL
SELECT '2013-Jun' as calendar_year_month,102 as month_since_2005 
ORDER BY month_since_2005 desc]]></query> 
      </component-definition> 
    </action-definition>
  
    <actions> 
      <condition><![CDATA[p_emailMode=='false']]></condition>  
      <action-definition> 
        <component-name>SecureFilterComponent</component-name>
        <action-type>Prompt/Secure Filter</action-type>
        <action-inputs> 
          <p_start_date type="string"/>  
          <p_end_date type="string"/>  
          <v_max_date type="string"/>  
          <v_min_date type="string"/>  
          <q_max_min_date type="result-set"/>  
          <p_output type="string"/>  
          <List_output_type type="string-list"/>  
          <p_start_month_since_2005 type="string"/>  
          <p_end_month_since_2005 type="string"/>  
          <p_date_mode type="string"/>  
          <list_date_mode type="string-list"/>  
          <query_result_list_months type="result-set"/> 
        </action-inputs>
        <action-resources> 
          <pageTemplate mapping="pageTemplate1"/> 
        </action-resources>
        <component-definition> 
          <selections> 
            <p_start_date filter="none" style="text-box"/>  
            <p_end_date filter="none" style="text-box"/>  
            <v_max_date style="select"> 
              <filter value-col-name="max_full_date" display-col-name="max_full_date">q_max_min_date</filter> 
            </v_max_date>  
            <v_min_date style="select"> 
              <filter value-col-name="min_full_date" display-col-name="min_full_date">q_max_min_date</filter> 
            </v_min_date>  
            <p_output style="select"> 
              <filter>List_output_type</filter> 
            </p_output>  
            <p_start_month_since_2005 style="select"> 
              <filter value-col-name="month_since_2005" display-col-name="calendar_year_month">query_result_list_months</filter> 
            </p_start_month_since_2005>  
            <p_end_month_since_2005 style="select"> 
              <filter value-col-name="month_since_2005" display-col-name="calendar_year_month">query_result_list_months</filter> 
            </p_end_month_since_2005>  
            <p_date_mode style="radio"> 
              <filter>list_date_mode</filter> 
            </p_date_mode> 
          </selections>  
          <xsl><![CDATA[pageTemplate]]></xsl> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Process Adnetwork IDs</action-type>
        <action-inputs> 
          <p_adnetwork_ids type="string"/> 
        </action-inputs>
        <action-outputs> 
          <p_adnetwork_id_list type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var p_adnetwork_id_list='0';
//convert measures
if(p_adnetwork_ids!= ''){
	if(p_adnetwork_ids instanceof Array){
		for(var i=0;i<p_adnetwork_ids.length;i++){
			if(i>0){
			p_adnetwork_id_list+=';';
			};
			p_adnetwork_id_list+=p_adnetwork_ids[i];
		};
	}else{
		p_adnetwork_id_list=p_adnetwork_ids;
	};
}]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Process Publisher IDs</action-type>
        <action-inputs> 
          <p_publisher_ids type="string"/> 
        </action-inputs>
        <action-outputs> 
          <p_publisher_id_list type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var p_publisher_id_list='0';
//convert measures
if(p_publisher_ids!= ''){
	if(p_publisher_ids instanceof Array){		
		p_publisher_id_list=p_publisher_ids.join(";");
	}else{
		p_publisher_id_list=p_publisher_ids;
	};
}]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Process Property IDs</action-type>
        <action-inputs> 
          <p_property_ids type="string"/> 
        </action-inputs>
        <action-outputs> 
          <p_property_id_list type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var p_property_id_list='0';
//convert measures
if(p_property_ids!= ''){
	if(p_property_ids instanceof Array){
		for(var i=0;i<p_property_ids.length;i++){
			if(i>0){
			p_property_id_list+=';';
			};
			p_property_id_list+=p_property_ids[i];
		};
	}else{
		p_property_id_list=p_property_ids;
	};
}]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Process Channel IDs</action-type>
        <action-inputs> 
          <p_channel_ids type="string"/> 
        </action-inputs>
        <action-outputs> 
          <p_channel_id_list type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var p_channel_id_list='0';
//convert measures
if(p_channel_ids!= ''){
	if(p_channel_ids instanceof Array){
		for(var i=0;i<p_channel_ids.length;i++){
			if(i>0){
			p_channel_id_list+=';';
			};
			p_channel_id_list+=p_channel_ids[i];
		};
	}else{
		p_channel_id_list=p_channel_ids;
	};
}]]></script> 
        </component-definition> 
      </action-definition>
  
      <actions> 
        <condition><![CDATA[p_date_mode=='date']]></condition>  
        <action-definition> 
          <component-name>JasperReportsComponent</component-name>
          <action-type>Jasper</action-type>
          <action-inputs> 
            <p_start_date type="string"/>  
            <p_end_date type="string"/>  
            <p_adnetwork_id_list type="string"/>  
            <output-type type="string" mapping="p_output"/>  
            <p_property_id_list type="string"/>  
            <p_channel_id_list type="string"/>  
            <p_publisher_id_list type="string"/>  
            <jndi type="string" mapping="connection_name"/> 
          </action-inputs>
          <action-resources> 
            <report-definition type="resource"/> 
          </action-resources>
          <component-definition/> 
        </action-definition>
 
      </actions>
  
      <actions> 
        <condition><![CDATA[p_date_mode=='month']]></condition>  
        <action-definition> 
          <component-name>JasperReportsComponent</component-name>
          <action-type>Jasper</action-type>
          <action-inputs> 
            <p_start_month_since_2005 type="string"/>  
            <p_end_month_since_2005 type="string"/>  
            <p_adnetwork_id_list type="string"/>  
            <p_property_id_list type="string"/>  
            <output-type type="string" mapping="p_output"/>  
            <p_channel_id_list type="string"/>  
            <p_publisher_id_list type="string"/>  
            <jndi type="string" mapping="connection_name"/> 
          </action-inputs>
          <action-resources> 
            <report-definition mapping="report-definition1"/> 
          </action-resources>
          <component-definition/> 
        </action-definition>
 
      </actions>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[p_emailMode=='true']]></condition>  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Process report input</action-type>
        <action-inputs> 
          <p_start_date type="string"/>  
          <p_end_date type="string"/>  
          <p_emailExportFileType type="string"/>  
          <parameter_string type="string"/>  
          <q_max_min_date type="result-set"/> 
        </action-inputs>
        <action-outputs> 
          <reportName type="string"/>  
          <p_start_date type="string"/>  
          <reportNameCsv type="string"/>  
          <p_mailTextMessage type="string"/>  
          <p_mailTitle type="string"/>  
          <p_adnetwork_id_list type="string"/>  
          <p_publisher_id_list type="string"/>  
          <p_property_id_list type="string"/>  
          <p_channel_id_list type="string"/>  
          <p_end_date type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var reportName='';
var p_date='';
var p_adnetwork_id_list='0';
var p_publisher_id_list='0';
var p_property_id_list='0';
var p_channel_id_list='0';
p_start_date=q_max_min_date.getValueAt(0,1);
p_end_date=p_start_date;
p_date=p_start_date;
var dateFormat=p_start_date;

//dateFormat=dateFormat.replace(/\-/gi,"");
reportName='Delivery_by_Adnetwork_Publisher_Property_Channel_V2.4_'+dateFormat+'.'+p_emailExportFileType;
reportNameCsv='Delivery_by_Adnetwork_Publisher_Property_Channel_V2.4_'+dateFormat+'.csv';
var p_mailTextMessage='Daily Delivery by Adnetwork Publisher Property Channel V2.4 on '+dateFormat;
var p_mailTitle='Delivery by Adnetwork Publisher Property Channel V2.4';]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JasperReportsComponent</component-name>
        <action-type>Jasper PDF</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connection_name"/>  
          <p_start_date type="string"/>  
          <p_end_date type="string"/>  
          <p_adnetwork_id_list type="string"/>  
          <p_publisher_id_list type="string"/>  
          <p_property_id_list type="string"/>  
          <p_channel_id_list type="string"/> 
        </action-inputs>
        <action-outputs> 
          <report-output type="content" mapping="report_ou_pdf"/> 
        </action-outputs>
        <component-definition> 
          <output-type><![CDATA[pdf]]></output-type> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JasperReportsComponent</component-name>
        <action-type>Jasper Csv</action-type>
        <action-inputs> 
          <p_start_date type="string"/>  
          <p_end_date type="string"/>  
          <p_adnetwork_id_list type="string"/>  
          <p_publisher_id_list type="string"/>  
          <p_property_id_list type="string"/>  
          <p_channel_id_list type="string"/>  
          <jndi type="string" mapping="connection_name"/> 
        </action-inputs>
        <action-outputs> 
          <report-output type="content" mapping="report_ou_csv"/> 
        </action-outputs>
        <component-definition> 
          <output-type><![CDATA[csv]]></output-type> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>EmailComponent</component-name>
        <action-type>E-Mail</action-type>
        <action-inputs> 
          <to type="string" mapping="p_mailTo"/>  
          <subject type="string" mapping="p_mailTitle"/>  
          <message-plain type="string" mapping="p_mailTextMessage"/>  
          <report_ou_pdf type="content"/>  
          <attach_name_1 type="string" mapping="reportName"/>  
          <report_ou_csv type="content"/>  
          <attach_name_2 type="string" mapping="reportNameCsv"/> 
        </action-inputs>
        <component-definition> 
          <attachment-ref name-param="attach_name_1" input-param="report_ou_pdf"/>  
          <attachment-ref name-param="attach_name_2" input-param="report_ou_csv"/> 
        </component-definition> 
      </action-definition>
 
    </actions>
 
  </actions> 
</action-sequence>