	<queryString>
		<![CDATA[SELECT   full_date
        ,(max (a.dt_lastchange)::timestamp with time zone AT TIME ZONE 'America/Los_Angeles')::timestamp with time zone AT TIME ZONE 'America/New_York'as dt_lastchange
        ,COALESCE(sum(case requests when -1 then 0 else requests end),0) as requests
        ,COALESCE(sum(case impressions when -1 then 0 else impressions end),0) as paid_impressions
        ,COALESCE(sum(case clicks when -1 then 0 else clicks end),0) as clicks
        ,sum(revenue) as net_revenue
        ,sum(case filled_requests when -1 then 0 else filled_requests end) as total_adcel_yes
        ,sum(case responses_count when -1 then 0 else responses_count end) as total_adcel_total
	,sum(today_estimate_revenue) as today_estimate_revenue
	,sum(event_count) as today_event_count
FROM adnetwork.daily_am_performance a
WHERE eastern_date_sk=$P{eastern_date_sk}::int
  AND is_active=true
GROUP BY full_date]]>
	</queryString>
