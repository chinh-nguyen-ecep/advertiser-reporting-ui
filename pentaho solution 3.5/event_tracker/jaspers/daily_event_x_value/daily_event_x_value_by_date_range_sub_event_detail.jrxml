	<queryString>
		<![CDATA[SELECT *
FROM (
SELECT	full_date,
	hour24_of_day,
	day_of_week,
	x_value,
	adimp_count,
	expandimp_count,
	adimp2_count,
	ctaclick_count
FROM evttracker.daily_event_x_value_report
WHERE is_active=true
  AND flight_id=$P{p_flight_id}
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
UNION ALL
SELECT	full_date,
	hour24_of_day,
	day_of_week,
	x_value,
	adimp_count,
	expandimp_count,
	adimp2_count,
	ctaclick_count
FROM evttracker.daily_event_x_value_report
WHERE $P{p_flight_id} = 'All Flight ID'
  AND is_active=true
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
) a
ORDER BY full_date, hour24_of_day, x_value]]>
