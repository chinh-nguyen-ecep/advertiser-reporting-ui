	<queryString>
		<![CDATA[select
'001' as Error_code
,io.io_orders_id::text as io_id
,io.coract_name::text as io_name
,io.revenue_goal::text as io_budget
,'Unknown' as adm_order_id
,'Unknown' as adm_order_name
,li.billing_line_items_id::text as io_line_item_id
,li.name::text as io_line_item_name
,li.budget::text as io_line_item_budget
,'Unknown' as total_adm_flight_budget
,'N/A' as budget_compare_difference
,li.quantity::text as io_line_item_quantity
,'Unknown' as total_adm_flight_quantity
,'N/A' as Quantity_compare_difference
from refer.adm_dim_io_orders io
inner join refer.adm_dim_io_lineitems li
on(li.dt_expire = '9999-12-31' and li.end_date_current::date >= '2014-01-01' and io.io_orders_id = li.insertion_order_id)
where io.dt_expire = '9999-12-31'
and io.io_orders_id not in(
select distinct insertion_order_id from refer.adm_dim_orders o
where o.dt_expire = '9999-12-31')
union
select
'002' as Error_code
,'Unknown' as Io_id
,'Unknown' as io_name
,'Unknown' as io_budget
,o.order_id::text as adm_order_id
,o.name_current::text as adm_order_name
,'Unknown' as io_line_items_id
,'Unknown' as io_line_item_name
,'Unknown' as io_line_item_budget
,sum.sum_flight_budget::text as total_adm_flight_budget
,'N/A' as budget_compare_difference
,'Unknown' as io_line_item_quantity
,sum.sum_flight_quantity::text as total_adm_flight_quantity
,'N/A' as Quantity_compare_difference
from refer.adm_dim_orders o
inner join (
select ---Sum flight budget and quantity at order level
f.order_id
,sum(f.budget) as sum_flight_budget
,sum(f.quantity) as sum_flight_quantity
from refer.adm_dim_flights f
where f.dt_expire = '9999-12-31'
and f.network_id = 2
and f.paused_at is null
and f.cancelled_at is null
and f.submitted_at is NOT null
and f.end_date_current::date >= '2014-01-01'
and f.specialty_id <>4
group by f.order_id
)sum
on(sum.order_id = o.order_id)
where o.dt_expire = '9999-12-31'
and o.order_id not in(select order_id from adsops.AdsOp_IO_ADM_exception_order_exclusion where is_active = 1)
and o.insertion_order_id not in(
select distinct io.io_orders_id from refer.adm_dim_io_orders io
where io.dt_expire = '9999-12-31')
union
select
'003' as Error_code
,io.io_orders_id::text as io_id
,io.coract_name::text as io_name
,io.revenue_goal::text as io_budget
,o.order_id::text as adm_order_id
,o.name_current::text as adm_order_name
,'Unknown' as io_line_items_id
,'Unknown' as io_line_item_name
,'Unknown' as io_line_item_budget
,sum.sum_flight_budget::text as total_adm_flight_budget
,'N/A' as budget_compare_difference
,'Unknown' as io_line_item_quantity
,sum.sum_flight_quantity::text as total_adm_flight_quantity
,'N/A' as Quantity_compare_difference
from refer.adm_dim_orders o
inner join (
select ---Sum flight budget and quantity at order level
f.order_id
,f.billing_line_item_id
,sum(f.budget) as sum_flight_budget
,sum(f.quantity) as sum_flight_quantity
from refer.adm_dim_flights f
where f.dt_expire = '9999-12-31'
and f.network_id = 2
and f.paused_at is null
and f.cancelled_at is null
and f.submitted_at is NOT null
and f.end_date_current::date >= '2014-01-01'
and f.specialty_id<>4
and f.billing_line_item_id not in(select distinct billing_line_items_id from refer.adm_dim_io_lineitems
where dt_expire = '9999-12-31')
group by f.order_id, f.billing_line_item_id
)sum
on(sum.order_id = o.order_id)
inner join refer.adm_dim_io_orders io
on(io.dt_expire = '9999-12-31' and io.io_orders_id = o.insertion_order_id)
where o.dt_expire = '9999-12-31'
and o.order_id not in(select order_id from adsops.AdsOp_IO_ADM_exception_order_exclusion where is_active = 1)
union
select
'004' as Error_code
,io.io_orders_id::text as io_id
,io.coract_name::text as io_name
,io.revenue_goal::text as io_budget
,o.order_id::text as adm_order_id
,o.name_current::text as adm_order_name
,li.billing_line_items_id::text as io_line_item_id
,li.name::text as io_line_item_name
,li.budget::text as io_line_item_budget
,'Unknown' as total_adm_flight_budget
,'N/A' as budget_compare_difference
,li.quantity::text as io_line_item_quantity
,'Unknown' as total_adm_flight_quantity
,'N/A' as Quantity_compare_difference
from refer.adm_dim_io_orders io
inner join refer.adm_dim_io_lineitems li
on(li.dt_expire = '9999-12-31' and li.end_date_current::date >= '2014-01-01'and io.io_orders_id = li.insertion_order_id)
inner join refer.adm_dim_orders o
on (o.dt_expire = '9999-12-31' and o.order_id not in(select order_id from adsops.AdsOp_IO_ADM_exception_order_exclusion where is_active = 1)
and io.io_orders_id = o.insertion_order_id)
where io.dt_expire = '9999-12-31'
and li.billing_line_items_id not in
(select distinct billing_line_item_id from refer.adm_dim_flights f
where f.dt_expire = '9999-12-31'
and f.network_id = 2)
union
select
Case when (a.difference_budget::double precision= 0 and a.difference_quantity::double precision = 0) then '000' else '005' end error_code
,a.*
from(
select
--'000' as Error_code
io.io_orders_id::text as io_id
,io.coract_name::text as io_name
,io.revenue_goal::text as io_budget
,o.order_id::text as adm_order_id
,o.name_current::text as adm_order_name
,li.billing_line_items_id::text as io_line_item_id
,li.name::text as io_line_item_name
,li.budget::text as io_line_item_budget
,sum.sum_flight_budget::text as total_adm_flight_budget
,(li.budget -sum.sum_flight_budget)::text  as Difference_budget
,li.quantity::text as io_line_item_quantity
,sum.sum_flight_quantity::text as total_adm_flight_quantity
,(li.quantity -sum.sum_flight_quantity)::text  as Difference_Quantity
from refer.adm_dim_orders o
inner join refer.adm_dim_io_orders io
on(io.dt_expire = '9999-12-31' and io.io_orders_id = o.insertion_order_id)
inner join refer.adm_dim_io_lineitems li
on(li.dt_expire = '9999-12-31' and li.end_date_current::date >= '2014-01-01' and li.insertion_order_id = o.insertion_order_id)
inner join (
select ---Sum flight budget and quantity at order level
f.order_id
,f.billing_line_item_id
,sum(f.budget) as sum_flight_budget
,sum(f.quantity) as sum_flight_quantity
from refer.adm_dim_flights f
where f.dt_expire = '9999-12-31'
and f.network_id = 2
and f.paused_at is null
and f.cancelled_at is null
and f.submitted_at is NOT null
--and f.end_date_current::date >= '2014-01-01'
group by f.order_id, f.billing_line_item_id
)sum
on(sum.order_id = o.order_id and sum.billing_line_item_id = li.billing_line_items_id)
where o.dt_expire = '9999-12-31'
and o.order_id not in(select order_id from adsops.AdsOp_IO_ADM_exception_order_exclusion where is_active = 1)
)a
union
select
'006' as Error_code
,'Unknown' as Io_id
,'Unknown' as io_name
,'Unknown' as io_budget
,o.order_id::text as adm_order_id
,o.name_current::text as adm_order_name
,'Unknown' as io_line_items_id
,'Unknown' as io_line_item_name
,'Unknown' as io_line_item_budget
,sum.sum_flight_budget::text as total_adm_flight_budget
,'N/A' as budget_compare_difference
,'Unknown' as io_line_item_quantity
,sum.sum_flight_quantity::text as total_adm_flight_quantity
,'N/A' as Quantity_compare_difference
from refer.adm_dim_orders o
inner join (
select ---Sum flight budget and quantity at order level
f.order_id
,f.billing_line_item_id
,sum(f.budget) as sum_flight_budget
,sum(f.quantity) as sum_flight_quantity
from refer.adm_dim_flights f
where f.dt_expire = '9999-12-31'
and f.network_id = 2
and f.paused_at is null
and f.cancelled_at is null
and f.submitted_at is NOT null
and f.end_date_current::date >= '2014-01-01'
and f.specialty_id<>4
and f.billing_line_item_id not in(select distinct billing_line_items_id from refer.adm_dim_io_lineitems
where dt_expire = '9999-12-31')
group by f.order_id, f.billing_line_item_id
)sum
on(sum.order_id = o.order_id)
where o.dt_expire = '9999-12-31'
and o.order_id not in(select order_id from adsops.AdsOp_IO_ADM_exception_order_exclusion where is_active = 1)
and o.insertion_order_id not in(select distinct io_orders_id from refer.adm_dim_io_orders where dt_expire = '9999-12-31')
order by error_code
;
]]>
	</queryString>
