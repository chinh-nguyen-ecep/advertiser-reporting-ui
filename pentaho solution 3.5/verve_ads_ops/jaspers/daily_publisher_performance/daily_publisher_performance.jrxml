	<queryString>
		<![CDATA[SELECT full_date as "Date",
       publisher_id as "Publisher ID"
	   , publisher_name as "Publisher Name"
       , property_id as "Property ID"
	   , property_name as "Property Name"
       , channel_id as "Channel ID"
	   , channel_name as "Channel Name"
       , SUM(impressions) as "Impressions"
       , SUM(clicks) as "Clicks"
       , SUM(cta_any) as "Cta Any"
       , SUM(e_refresh) as "E Refresh"
       , SUM(e_cta_tap_refresh) as "E Cta Tap Refresh"
	   , SUM(e_cta_map) as "E Cta Map"
       , SUM(e_cta_lp) as "E Cta Lp"
       , SUM(e_cta_call) as "E Cta Call"
       , SUM(e_cta_cal) as "E Cta Cal"
       , SUM(e_cta_video) as "E Cta Video"
       , SUM(e_cta_face) as "E Cta Face"
       , SUM(e_cta_twit) as "E Cta Twit"
       , SUM(e_cta_email) as "E Cta Email"
       , SUM(e_cta_txt) as "E Cta Txt"
       , SUM(e_cta_tap) as "E Cta Tap"
       , SUM(e_cta_swipe) as "E Cta Swipe"
       , SUM(e_cta_save_coupon) as "E Cta Save Coupon"
       , SUM(e_cta_send_coupon) as "E Cta send Coupon"
       , SUM(e_duration) as "E Duration"
       , SUM(e_error) as "E Error"
       , SUM(e_video0) as "E Video0"
       , SUM(e_video25) as "E Video25"
       , SUM(e_video50) as "E Video50"
       , SUM(e_video75) as "E Video75"
       , SUM(e_video100) as "E Video100"
       , max(dt_lastchange) as dt_lastchange
  FROM adsops.daily_agg_publisher_performance
  WHERE is_active =true
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
  GROUP BY
  full_date,
       publisher_id, publisher_name
       , property_id, property_name
       , channel_id, channel_name
  ORDER BY full_date,publisher_name,property_name ,channel_name;]]>
	</queryString>
