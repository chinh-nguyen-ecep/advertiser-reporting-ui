<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="detail_by_placement" language="groovy" pageWidth="1350" pageHeight="555" orientation="Landscape" columnWidth="1350" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="0.9330147604194666"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="eastern_date_sk" class="java.lang.Integer"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="totalRevenue" class="java.lang.Double"/>
	<queryString>
		<![CDATA[SELECT sum(a.revenue_count) FROM
(
	SELECT
	yesterday_rank,
	order_id,
	revenue as revenue_count
	FROM adm.daily_agg_revenue_by_order_beta
	WHERE is_active=true
	AND eastern_date_sk=$P{eastern_date_sk}::int
	AND yesterday_rank > 0
	ORDER BY
	yesterday_rank
	LIMIT 20
) a]]>
	</queryString>
	<field name="sum" class="java.lang.Double"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<pageHeader>
		<band height="20">
			<staticText>
				<reportElement x="0" y="0" width="198" height="20"/>
				<textElement verticalAlignment="Top">
					<font isBold="true"/>
				</textElement>
				<text><![CDATA[Yesterday Top 20 Revenues by Order:]]></text>
			</staticText>
			<textField pattern="#,##0.00;-#,##0.00">
				<reportElement x="198" y="0" width="70" height="20"/>
				<textElement verticalAlignment="Top">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.Double"><![CDATA[$F{sum}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement x="268" y="0" width="58" height="20"/>
				<textElement textAlignment="Right" verticalAlignment="Top">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[new DecimalFormat("0.00").format(new Double(($F{sum}.doubleValue()*100)/$P{totalRevenue})) + "%"]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<detail>
		<band height="29" splitType="Stretch">
			<subreport>
				<reportElement x="0" y="0" width="1350" height="29"/>
				<subreportParameter name="eastern_date_sk">
					<subreportParameterExpression><![CDATA[$P{eastern_date_sk}]]></subreportParameterExpression>
				</subreportParameter>
				<connectionExpression><![CDATA[$P{REPORT_CONNECTION}]]></connectionExpression>
				<subreportExpression class="java.lang.String"><![CDATA[$P{SUBREPORT_DIR} + "top_20_revenue_campaign_subreport2.jasper"]]></subreportExpression>
			</subreport>
		</band>
	</detail>
