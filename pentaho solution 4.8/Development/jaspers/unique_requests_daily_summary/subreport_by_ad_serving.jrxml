	<queryString>
		<![CDATA[SELECT
a.eastern_date_sk,
a.full_date,
sum(a.fullfilled_code_count) as fullfilled_code_count,
sum(a.fullfilled_code_y) as fullfilled_code_y,
sum(a.fullfilled_code_n) as fullfilled_code_n,
