	a.is_active=true
	AND a.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
	AND (a.flight_id IN (select unnest(string_to_array($P{flight_ids_string}, ','))) OR '0'=$P{flight_ids_string})
