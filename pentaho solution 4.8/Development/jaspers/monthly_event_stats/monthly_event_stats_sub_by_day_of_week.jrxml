	<queryString>
		<![CDATA[SELECT day_of_week, sum(event_count) as event_count
  FROM evttracker.monthly_event_stats_by_date
  WHERE flight_id=$P{p_flight_id} AND
  event_name=$P{p_event_name} AND
  calendar_year_month=$P{p_calendar_year_month} AND
  is_active=true
  GROUP BY day_of_week
  ORDER BY day_of_week]]>
