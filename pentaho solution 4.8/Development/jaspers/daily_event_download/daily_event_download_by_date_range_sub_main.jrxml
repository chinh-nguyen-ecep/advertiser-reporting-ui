<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="daily_event_download_by_date_range_sub_main" language="groovy" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="p_flight_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="p_start_date" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("9999-12-31")]]></defaultValueExpression>
	</parameter>
	<parameter name="p_end_date" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("9999-12-31")]]></defaultValueExpression>
	</parameter>
	<parameter name="AdClick_total" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer("1")]]></defaultValueExpression>
	</parameter>
	<parameter name="Download_total" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer("1")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	a.full_date,
	a.day_of_week,
	a.bundle_package,
	a.adclick_count,
	a.download_count
  FROM evttracker.daily_event_download a
  WHERE
	a.is_active=true AND
	a.full_date>=$P{p_start_date}::date AND
	a.full_date<=$P{p_end_date}::date AND
	a.flight_id=$P{p_flight_id}
ORDER BY a.full_date,a.bundle_package]]>
	</queryString>
	<field name="full_date" class="java.sql.Date"/>
	<field name="day_of_week" class="java.lang.String"/>
	<field name="bundle_package" class="java.lang.String"/>
	<field name="adclick_count" class="java.lang.Integer"/>
	<field name="download_count" class="java.lang.Integer"/>
	<variable name="total_adclick" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{adclick_count}]]></variableExpression>
	</variable>
	<variable name="total_download" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{download_count}]]></variableExpression>
	</variable>
