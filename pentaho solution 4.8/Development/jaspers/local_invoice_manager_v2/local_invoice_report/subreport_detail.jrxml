	<queryString>
		<![CDATA[SELECT
inv_detail_id
,account_name
,adm_partner_id
,adm_partner_name as "Site"
,account_partner_map_id
,UPPER(flight_metric) as "Type"
,adname as "Description"
,verve_share as "verve_share"
,SUM(billable_units) as "Quantity"
,SUM(gross_revenue) as "Gross"
FROM accounting.ba_inv_local_report_detail
WHERE inv_mmyy=$P{p_month_year}
AND status = 'ACTIVE'
--AND gross_revenue >0
AND inv_detail_id =  $P{p_inv_detail_id}--pass specific invoice number
AND adm_partner_name =$P{p_site}
GROUP BY
inv_detail_id
,account_name
,adm_partner_id
,adm_partner_name
,flight_metric
,adname
,account_partner_map_id
,verve_share
ORDER BY inv_detail_id, account_name, adm_partner_id;]]>
