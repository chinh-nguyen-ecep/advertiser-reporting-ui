  <actions> 
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
      <action-type>Get list of group</action-type>
      <action-outputs> 
        <query-result type="result-set" mapping="q_group"/>  
        <parent1_name type="string"/>  
        <row_order type="string"/> 
      </action-outputs>
      <component-definition> 
        <jndi><![CDATA[dw1]]></jndi>  
        <query><![CDATA[SELECT a.*
FROM (
SELECT 'All Parents' as parent1_name, 0 as row_order
UNION
SELECT parent1_name, 1 as row_order
FROM dw.daily_agg_group_act_all
WHERE organic = true AND is_active=true GROUP by parent1_name
) a
ORDER BY row_order, parent1_name;]]></query>  
        <live><![CDATA[false]]></live> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
      <action-type>Get list of partner</action-type>
      <action-outputs> 
        <query-result type="result-set" mapping="q_partner"/>  
        <partner_name type="string"/>  
        <row_order type="string"/> 
      </action-outputs>
      <component-definition> 
        <jndi><![CDATA[dw1]]></jndi>  
        <query><![CDATA[SELECT a.*
FROM (
SELECT 'All Partners' as partner_name, 0 as row_order
UNION
SELECT partner_name, 1 as row_order
FROM dw.daily_agg_partner_act_all
WHERE  organic=true And is_active=true GROUP by partner_name
) a
ORDER BY row_order, partner_name;]]></query>  
        <live><![CDATA[false]]></live> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
