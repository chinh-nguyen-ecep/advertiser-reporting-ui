    <actions> 
      <condition><![CDATA[p_mode=='date']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Get date list</action-type>
        <action-inputs> 
          <p_start_date type="string"/>  
          <p_end_date type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="q_date_list"/> 
        </action-outputs>
        <component-definition> 
          <jndi><![CDATA[dw1]]></jndi>  
          <live><![CDATA[true]]></live>  
          <query><![CDATA[SELECT 
 full_date
FROM 
  dw.daily_agg_group_act_all
WHERE 
  full_date BETWEEN '{p_start_date}' AND '{p_end_date}' GROUP BY full_date ORDER BY full_date ASC;]]></query> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Convert Parameter to OLAP script</action-type>
      <action-inputs> 
        <p_measures type="string"/>  
        <device type="string-list"/>  
        <q_date_list type="result-set"/>  
        <p_start_month type="string"/>  
        <p_end_month type="string"/>  
        <q_calendar_year_month type="result-set"/>  
        <p_dimension_mode type="string"/>  
        <p_group type="string"/>  
        <p_partner type="string"/>  
        <p_parent_mode type="string"/>  
        <p_mode type="string"/> 
      </action-inputs>
      <action-outputs> 
        <t_measures type="string"/>  
        <t_device type="string"/>  
        <t_date_list type="string"/>  
        <t_month_list type="string"/>  
        <t_parent type="string"/>  
        <tp_parent type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var t_measures;
var t_device;
var t_date_list;
var t_month_list;
var t_parent;
var t_parent;
var tp_parent;
//set default value
t_measures='';
t_device='';
t_date_list='';
t_month_list='';
t_parent='';
tp_parent='';

//convert measures
if(p_mode=='date'){
if(p_measures instanceof Array){
	for(var i=0;i<p_measures.length;i++){
		if(i>0){
		t_measures+=',';
		};
		if(p_measures[i]=='Page view'){
			t_measures+='[Measures].[Estimate page view]';
		}else if(p_measures[i]=='Unique view'){
			t_measures+='[Measures].[Estimate unique view]';
		}else if(p_measures[i]=='Unique visit'){
			t_measures+='[Measures].[Estimate unique visit]';
		};
		
	};
}else{
		if(p_measures=='Page view'){
			//p_measures ='Estimate page view';
			t_measures+='[Measures].[Estimate page view]';
		}else if(p_measures=='Unique view'){
			//p_measures ='Estimate unique view';
			t_measures+='[Measures].[Estimate unique view]';
		}else if(p_measures=='Unique visit'){
			//p_measures ='Estimate unique visit';
			t_measures+='[Measures].[Estimate unique visit]';
		};
}
}else{
if(p_measures instanceof Array){
	for(var i=0;i<p_measures.length;i++){
		if(i>0){
		t_measures+=',';
		};
		t_measures+='[Measures].['+p_measures[i]+']';
	};
}else{
	t_measures+='[Measures].['+p_measures+']';
}
};
//convert dimension
var fistTextOfDimension;
if(p_dimension_mode=='Device'){
	fistTextOfDimension='Device type';
	
}else if(p_dimension_mode=='Partner'){
	fistTextOfDimension='Partner';
	
}else if(p_dimension_mode=='Portal'){
	fistTextOfDimension='Portal';
	
}else if(p_dimension_mode=='Group'){
	fistTextOfDimension='Parent';
	
}else if(p_dimension_mode=='ContentCategory'){
	fistTextOfDimension='Category';
	
};
if(device instanceof Array){
	for(var i=0;i<device.length;i++){
		if(i>0){
		t_device+=',';
		};
		t_device+='['+fistTextOfDimension+'].['+device[i]+']';
	};
}else{
	t_device+='['+fistTextOfDimension+'].['+device+']';
};


//convert parent
if(p_parent_mode=='Group'){
	if(p_group instanceof Array){
		for(var i=0;i<p_group.length;i++){
			if(i>0){
			t_parent+=',';
			};
			t_parent+='[Parent].['+p_group[i]+']';
		};
	}else{
		t_parent+='[Parent].['+p_group+']';
		tp_parent+=p_group;
	};
}else if(p_parent_mode=='Partner'){
	if(p_partner instanceof Array){
		for(var i=0;i<p_partner.length;i++){
			if(i>0){
			t_parent+=',';
			};
			t_parent+='[Partner].['+p_partner[i]+']';
		};
	}else{
		t_parent+='[Partner].['+p_partner+']';
		tp_parent+=p_partner;
	};
};

//convert date list
var countDateList=q_date_list.getRowCount();
for(var i=0;i<countDateList;i++){
		if(i>0){
			t_date_list+=',';
		};
		t_date_list+='[Date].['+q_date_list.getValueAt(i,0)+']';		
	};

//convert month list
var minRage;
var maxRage;
if(p_start_month<p_end_month){
	minRage=p_start_month;
	maxRage=p_end_month;
}else{
	minRage=p_end_month;
	maxRage=p_start_month;
};

for(var i=maxRage;i>=minRage;i--){
		if(i<maxRage){
			t_month_list+=',';
		};
		t_month_list+='[Year month].['+q_calendar_year_month.getValueAt(i,1)+']';
				
	};]]></script> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Get user choose mode</action-type>
      <action-inputs> 
        <p_measures type="string"/>  
        <device type="string-list"/> 
      </action-inputs>
      <action-outputs> 
        <v_user_choose_mode type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var v_user_choose_mode;
var measures;
var dimension;

//v_user_choose_mode='multiDimensions';

measures=typeof(p_measures);
dimension=typeof(device);

if( measures == 'string' && dimension == 'object'){
	v_user_choose_mode='multiDimensions';
}else if( measures == 'object' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'string' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'object' && dimension == 'object'){
	v_user_choose_mode = 'multiDeviceMeasures';
}]]></script> 
      </component-definition> 
    </action-definition>
  
    <actions> 
      <condition><![CDATA[p_report_type=='Page view count']]></condition>  
      <actions> 
        <condition><![CDATA[p_dimension_mode=='Portal']]></condition>  
        <actions> 
          <condition><![CDATA[v_user_choose_mode == 'multiDeviceMeasures']]></condition>  
          <action-definition> 
            <component-name>JavascriptRule</component-name>
            <action-type>Make OLAP query</action-type>
            <action-inputs> 
              <v_chart_title type="string"/>  
              <p_measures type="string"/>  
              <p_dimension_mode type="string"/>  
              <p_mode type="string"/>  
              <t_measures type="string"/>  
              <t_device type="string"/>  
              <t_date_list type="string"/>  
              <t_month_list type="string"/>  
              <t_parent type="string"/> 
            </action-inputs>
            <action-outputs> 
              <queryOLAP type="string"/>  
              <v_chart_title type="string"/> 
            </action-outputs>
            <component-definition> 
              <script><![CDATA[v_chart_title=p_measures[0]+' by '+p_dimension_mode+' on '+ tp_parent;

if(p_mode=='date'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_date_list;
	queryOLAP+='} ON ROWS from [daily_agg_portal_act_all] '; 
	queryOLAP+='where Crossjoin(Crossjoin({[IsActive].[true]}, {'+t_parent+'}), {[Organic].[true]})';
       //queryOLAP+='where Crossjoin({[IsActive].[true]}, {[Organic].[true]})';
}else if(p_mode=='month'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {';
	queryOLAP+=t_device;
	queryOLAP+='}) ON COLUMNS,  {';
	queryOLAP+=t_month_list;
	queryOLAP+='} ON ROWS from [monthly_agg_partner_act_portal] '; 
	queryOLAP+='where Crossjoin(Crossjoin({[IsActive].[true]}, {'+t_parent+'}), {[Organic].[true]})';
      // queryOLAP+='where Crossjoin({[IsActive].[true]}, {[Organic].[true]})';
};]]></script> 
