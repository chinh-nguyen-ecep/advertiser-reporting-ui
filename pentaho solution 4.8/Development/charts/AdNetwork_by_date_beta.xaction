        </selections>  
        <xsl><![CDATA[pageTemplate]]></xsl> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>processQueryFromInput</action-type>
      <action-inputs> 
        <p_mode type="string"/>  
        <p_start_date type="string"/>  
        <p_end_date type="string"/>  
        <p_start_month type="string"/>  
        <p_end_month type="string"/>  
        <device type="string-list"/>  
        <p_network type="string"/>  
        <p_dimension_mode type="string"/>  
        <p_measures type="string"/> 
      </action-inputs>
      <action-outputs> 
        <sqlQuery type="string"/>  
        <agg type="string"/>  
        <fields type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var sqlQuery='';
var agg='';
var fields='';
var st_from='';

st_from="adstraffic.daily_ad_serving_stats";


var p_measures_map_to_report_column_name={
	'fullfilled_code_y':'Yes Ad responses',
	'fullfilled_code_count':'Total Ad responses',
	'fullfilled_code_n':'No Ad responses',
	'fullfilled_code_e':'Error Ad responses',
	'fullfilled_code_t':'Timeout Ad responses',
}
var p_dimension_mode_map_to_db_column_name={
	'Partner':['partner_id','partner_name'],
	'Portal':['portal_id','portal_name'],
	'Group':['parent1_id','parent1_name']
}
var the_dimension=p_dimension_mode_map_to_db_column_name[p_dimension_mode];

//check select mode
var v_user_choose_mode;
var measures;
var dimension;
measures=typeof(p_measures);
dimension=typeof(device);
if( measures == 'string' && dimension == 'object'){
	v_user_choose_mode='multiDimensions';
}else if( measures == 'object' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'string' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'object' && dimension == 'object'){
	v_user_choose_mode = 'multiDeviceMeasures';
}

if(v_user_choose_mode=='multiMeasures'){	
	//measures
	var measures='';
	var temp_summaries='';
	var temp_fields='';
	if(p_measures instanceof Array){
		for(var i=0;i<p_measures.length;i++){
			if(i>0){measures+=',';temp_summaries+=',';temp_fields+=','}
			var column_name=p_measures_map_to_report_column_name[p_measures[i]];
			measures+='SUM('+p_measures[i]+') as "'+column_name+'"';
			temp_summaries+='"'+column_name+'"';
			temp_fields+='{"name": "'+column_name+'","type": "integer","rowLabelable": false,"summarizable": "sum"}';
		}	
	}else{
		var column_name=p_measures_map_to_report_column_name[p_measures];
		measures+='SUM('+p_measures+') as "'+column_name+'"';
		temp_summaries+='"'+column_name+'"';
		temp_fields+='{"name": "'+column_name+'","type": "integer","rowLabelable": false,"summarizable": "sum"}';
	};	
	
	if(p_mode=='date'){
		sqlQuery='SELECT full_date as "Date",'+measures+' '+
		'FROM '+st_from+' '+
		'WHERE is_active=true '+
		'AND full_date BETWEEN \''+p_start_date+'\'::date AND \''+p_end_date+'\'::date '+
		'AND ('+the_dimension[0]+'='+device+' OR 0='+device+') '+
		'AND (ad_network_id='+p_network+' OR '+'0='+p_network+') '+
		'GROUP BY full_date '+
		'ORDER BY full_date';	
		agg='{"rowLabels":["Date"], "columnLabels":[],"summaries":['+temp_summaries+']'+'}';
		fields='['+
				'{"name": "Date","type": "date","rowLabelable": false},'+temp_fields+
				']';
	}else if(p_mode=='month'){
		sqlQuery='SELECT calendar_year_month as "Month",'+measures+' '+
		'FROM '+st_from+' '+
		'WHERE is_active=true '+
		'AND month_since_2005 BETWEEN '+p_start_month+' AND '+p_end_month+' '+
		'AND ('+the_dimension[0]+'='+device+' OR 0='+device+') '+
		'AND (ad_network_id='+p_network+' OR '+'0='+p_network+') '+
		'GROUP BY month_since_2005,calendar_year_month '+
		'ORDER BY month_since_2005';		
		agg='{"rowLabels":["Month"], "columnLabels":[],"summaries":['+temp_summaries+']'+'}';
		fields='['+
				'{"name": "Month","type": "month","rowLabelable": false},'+temp_fields+
				']';
	}
	

}else{
	//measures
	var measures='';
	var temp_summaries='';
	var temp_fields='';
	if(p_measures instanceof Array){		
		var column_name=p_measures_map_to_report_column_name[p_measures[0]];
		measures+='SUM((CASE WHEN '+p_measures[0]+' IS NULL THEN 0 ELSE '+p_measures[0]+' END)) as "'+column_name+'"';
		temp_summaries+='"'+column_name+'"';
		temp_fields+='{"name": "'+column_name+'","type": "integer","rowLabelable": false,"summarizable": "sum"}';			
	}else{
		var column_name=p_measures_map_to_report_column_name[p_measures];
		measures+='SUM((CASE WHEN '+p_measures+' IS NULL THEN 0 ELSE '+p_measures+' END)) as "'+column_name+'"';
		temp_summaries+='"'+column_name+'"';
		temp_fields+='{"name": "'+column_name+'","type": "integer","rowLabelable": false,"summarizable": "sum"}';
	};	
	
	if(p_mode=='date'){
		sqlQuery='SELECT full_date as "Date",'+the_dimension[1]+' as "Dimension",'+measures+' '+
		'FROM '+st_from+' '+
		'WHERE is_active=true '+
		'AND full_date BETWEEN \''+p_start_date+'\'::date AND \''+p_end_date+'\'::date '+
		'AND '+the_dimension[0]+' IN ('+device.join(',')+') '+
		'AND (ad_network_id='+p_network+' OR '+'0='+p_network+') '+
		'GROUP BY full_date,'+the_dimension[1]+' '+
		'ORDER BY full_date,'+the_dimension[1]+' ';	
		
		agg='{"rowLabels":["Date"], "columnLabels":["Dimension"],"summaries":['+temp_summaries+']'+'}';
		fields='['+
				'{"name": "Date","type": "date","rowLabelable": false},'+
				'{"name": "Dimension","type": "string","rowLabelable": false},'+
				temp_fields+				
				']';
	}else if(p_mode=='month'){
		sqlQuery='SELECT calendar_year_month as "Month",'+the_dimension[1]+' as "Dimension",'+measures+' '+
		'FROM '+st_from+' '+
		'WHERE is_active=true '+
		'AND month_since_2005 BETWEEN '+p_start_month+' AND '+p_end_month+' '+
		'AND '+the_dimension[0]+' IN ('+device.join(',')+') '+
		'AND (ad_network_id='+p_network+' OR '+'0='+p_network+') '+
		'GROUP BY calendar_year_month,month_since_2005,'+the_dimension[1]+' '+
		'ORDER BY month_since_2005,'+the_dimension[1]+'';	
		
		agg='{"rowLabels":["Month"], "columnLabels":["Dimension"],"summaries":['+temp_summaries+']'+'}';
		fields='['+
				'{"name": "Month","type": "month","rowLabelable": false},'+
				'{"name": "Dimension","type": "string","rowLabelable": false},'+
				temp_fields+				
				']';
	}
}]]></script>
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>SQLLookupRule</component-name>
      <action-type>Relational</action-type>
      <action-inputs> 
        <query type="string" mapping="sqlQuery"/> 
      </action-inputs>
      <action-outputs> 
        <query-result type="result-set" mapping="query_result_chartData"/> 
      </action-outputs>
      <component-definition> 
        <jndi><![CDATA[verveReportConnection]]></jndi>  
        <live><![CDATA[true]]></live> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>processChartData</action-type>
      <action-inputs> 
        <query_result_chartData type="result-set"/> 
      </action-inputs>
      <action-outputs> 
        <data type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var data='';
var result_set=query_result_chartData;
var meta = result_set.getMetaData();      // Returns an IPentahoMetaData object
var colCount = meta.getColumnCount();
var colHeaders = meta.getColumnHeaders(); //getColumnHeaders() returns object[][]
var rowHeaders = meta.getRowHeaders(); 

data='[';

//process column name
data+='[';
for ( var i = 0 ; i < colCount ; i++ ){
	if(i>0){data+=',';}
	data+='\\"'+colHeaders[0][i]+'\\"';
}
data+=']';
//end
//process data item
for(var i=0;i<result_set.getRowCount();i++){
	data+=',[';
	for(var j=0;j<result_set.getColumnCount();j++){ 
		if(j>0){data+=','}
		data+='\\"'+result_set.getValueAt(i,j)+'\\"';
	}
	data+=']';
}
//end
data+=']';]]></script> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>TemplateComponent</component-name>
      <action-type>Message Template</action-type>
      <action-inputs> 
        <agg type="string"/>  
        <v_chart_sub_title type="string"/>  
        <data type="string"/>  
        <chart_type type="string"/>  
        <chartSingleMode type="string"/>  
        <p_chart_polar type="string"/>  
        <v_chart_title type="string"/>  
        <fields type="string"/> 
      </action-inputs>
      <action-resources/>
      <action-outputs> 
        <output-message type="string" mapping="webapp"/> 
      </action-outputs>
      <component-definition> 
        <template><![CDATA[{
			"chartTitle":"{v_chart_title}",
			"chartSubTitle":"{v_chart_sub_title}",
			"chartType":"{chart_type}",
			"chart_polar": {p_chart_polar},
			"chartSingleMode": {chartSingleMode},
			"chartData":"{data}",
			"agg":{agg},
			"fields":{fields},
			"sqlQuery":""
		}]]></template> 
      </component-definition> 
    </action-definition>
 
  </actions> 
</action-sequence>