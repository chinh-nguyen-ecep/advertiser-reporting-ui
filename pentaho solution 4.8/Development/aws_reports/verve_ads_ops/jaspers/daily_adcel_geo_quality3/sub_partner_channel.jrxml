	</parameter>
	<parameter name="p_end_date" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("9999-12-31")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.*
FROM
(
--All partners
SELECT
	eastern_date_sk
	, a.full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, b.channel_id
	, d.name_current as channel_name
	, SUM(trxids_count) as total_imp
	, CASE WHEN SUM(trxids_count) >0 THEN SUM(filled_count)::float/SUM(trxids_count) ELSE 0 END as fill_rate
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_0)::float/SUM(filled_count) ELSE 0 END as na_no_score
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_5)::float/SUM(filled_count) ELSE 0 END as foreign_content_ip_score_5
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_10)::float/SUM(filled_count) ELSE 0 END as mobile_ip_score_10
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_20)::float/SUM(filled_count) ELSE 0 END as content_location_score_20
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_45)::float/SUM(filled_count) ELSE 0 END as us_postal_score_45
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_50)::float/SUM(filled_count) ELSE 0 END as non_us_terr_ip_score_50
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_100)::float/SUM(filled_count) ELSE 0 END as lat_long_score_100
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_1_48)::float/SUM(filled_count) ELSE 0 END as quality_1_48
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_49_50)::float/SUM(filled_count) ELSE 0 END as quality_49_50
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_51_100)::float/SUM(filled_count) ELSE 0 END as quality_51_100
	, CASE WHEN SUM(filled_count)>0 THEN SUM(country_us)::float/SUM(filled_count) ELSE 0 END as us
	, CASE WHEN SUM(filled_count)>0 THEN SUM(country_international)::float/SUM(filled_count) ELSE 0 END as international
FROM adstraffic.daily_geo_quality a
LEFT JOIN refer.adm_mapping_portals b ON b.dt_expire='9999-12-31' AND b.portal_id=a.portal_id
INNER JOIN refer.date_dim c ON c.date_sk=a.eastern_date_sk AND c.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
LEFT JOIN refer.adm_channel_dim d ON d.dt_expire='9999-12-31' AND d.channel_id=b.channel_id
WHERE is_active=true
AND (partner_id IN (select unnest(string_to_array($P{p_partner_id_list}, ';'))::int) OR '0'=$P{p_partner_id_list})
GROUP BY eastern_date_sk,a.full_date,publisher_id,partner_id,publisher_name,partner_name,b.channel_id, channel_name
UNION
--All publisher
SELECT
	eastern_date_sk
	, a.full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, b.channel_id
	, d.name_current as channel_name
	, SUM(trxids_count) as total_imp
	, CASE WHEN SUM(trxids_count) >0 THEN SUM(filled_count)::float/SUM(trxids_count) ELSE 0 END as fill_rate
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_0)::float/SUM(filled_count) ELSE 0 END as na_no_score
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_5)::float/SUM(filled_count) ELSE 0 END as foreign_content_ip_score_5
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_10)::float/SUM(filled_count) ELSE 0 END as mobile_ip_score_10
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_20)::float/SUM(filled_count) ELSE 0 END as content_location_score_20
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_45)::float/SUM(filled_count) ELSE 0 END as us_postal_score_45
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_50)::float/SUM(filled_count) ELSE 0 END as non_us_terr_ip_score_50
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_100)::float/SUM(filled_count) ELSE 0 END as lat_long_score_100
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_1_48)::float/SUM(filled_count) ELSE 0 END as quality_1_48
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_49_50)::float/SUM(filled_count) ELSE 0 END as quality_49_50
	, CASE WHEN SUM(filled_count)>0 THEN SUM(location_quality_51_100)::float/SUM(filled_count) ELSE 0 END as quality_51_100
	, CASE WHEN SUM(filled_count)>0 THEN SUM(country_us)::float/SUM(filled_count) ELSE 0 END as us
	, CASE WHEN SUM(filled_count)>0 THEN SUM(country_international)::float/SUM(filled_count) ELSE 0 END as international
FROM adstraffic.daily_geo_quality a
LEFT JOIN refer.adm_mapping_portals b ON b.dt_expire='9999-12-31' AND b.portal_id=a.portal_id
INNER JOIN refer.date_dim c ON c.date_sk=a.eastern_date_sk AND c.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
LEFT JOIN refer.adm_channel_dim d ON d.dt_expire='9999-12-31' AND d.channel_id=b.channel_id
WHERE is_active=true
AND (parent1_id IN (select unnest(string_to_array($P{p_publisher_id_list}, ';'))::int) OR '0'=$P{p_publisher_id_list})
GROUP BY eastern_date_sk,a.full_date,publisher_id,partner_id,publisher_name,partner_name,b.channel_id, channel_name
)a
ORDER BY eastern_date_sk,publisher_name,partner_name, channel_name]]>
	</queryString>
