	<queryString>
		<![CDATA[select a.name_current,a.placement_id,a.impressions_count,a.clicks_count,a.revenue_count,b.impressions_count_1,b.clicks_count_1,b.revenue_count_1
,c.impressions_count_7,c.clicks_count_7,c.revenue_count_7
,d.impressions_count_28,d.clicks_count_28,d.revenue_count_28
from (( SELECT
a.eastern_date_sk,
a.full_date,
b.name_current,
a.placement_id,
sum(a.impressions) as impressions_count,
sum(a.clicks) as clicks_count,
sum(a.revenue) as revenue_count
FROM adm.daily_agg_adm_data_feed a,refer.adm_placement_dim b
WHERE a.is_active = true
AND a.placement_id=b.placement_id
AND b.active=true
AND b.dt_expire='9999-12-31'
AND a.eastern_date_sk = $P{eastern_date_sk}
GROUP BY
a.eastern_date_sk,
a.full_date,
a.placement_id,
b.name_current
ORDER BY
b.name_current asc,
impressions_count desc,
clicks_count desc,
revenue_count desc ) a left join
( SELECT
a.placement_id,
sum(a.impressions) as impressions_count_1,
sum(a.clicks) as clicks_count_1,
sum(a.revenue) as revenue_count_1
FROM adm.daily_agg_adm_data_feed a,refer.adm_placement_dim b
WHERE a.is_active = true
AND a.placement_id=b.placement_id
AND b.active=true
AND b.dt_expire='9999-12-31'
AND a.eastern_date_sk = $P{eastern_date_sk}-1
GROUP BY
a.eastern_date_sk,
a.placement_id
) b on a.placement_id=b.placement_id left join
(SELECT
a.placement_id,
sum(a.impressions) as impressions_count_7,
sum(a.clicks) as clicks_count_7,
sum(a.revenue) as revenue_count_7
FROM adm.daily_agg_adm_data_feed a,refer.adm_placement_dim b
WHERE a.is_active = true
AND a.placement_id=b.placement_id
AND b.active=true
AND b.dt_expire='9999-12-31'
AND a.eastern_date_sk = $P{eastern_date_sk}-7
GROUP BY
a.eastern_date_sk,
a.placement_id
) c on b.placement_id=c.placement_id left join
(SELECT
a.placement_id,
sum(a.impressions) as impressions_count_28,
sum(a.clicks) as clicks_count_28,
sum(a.revenue) as revenue_count_28
FROM adm.daily_agg_adm_data_feed a,refer.adm_placement_dim b
WHERE a.is_active = true
AND a.placement_id=b.placement_id
AND b.active=true
AND b.dt_expire='9999-12-31'
AND a.eastern_date_sk = $P{eastern_date_sk}-28
GROUP BY
a.eastern_date_sk,
a.placement_id
) d on c.placement_id=d.placement_id
)]]>
	</queryString>
