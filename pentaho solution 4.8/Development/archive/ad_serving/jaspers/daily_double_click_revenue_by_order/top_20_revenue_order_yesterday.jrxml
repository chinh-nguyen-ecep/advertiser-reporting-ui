<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="detail_by_placement" language="groovy" pageWidth="1250" pageHeight="555" orientation="Landscape" columnWidth="1250" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="2.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="eastern_date_sk" class="java.lang.Integer"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["D:\\Ecep\\Document-Tailieu\\Pentaho\\biserver-ce\\pentaho-solutions\\ad_serving\\jaspers\\daily_double_click_revenue_by_order\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="totalRevenue" class="java.lang.Double"/>
	<queryString>
		<![CDATA[SELECT sum(a.revenue_count) FROM (
SELECT
a.eastern_date_sk,
a.full_date,
b.name_current,
a.order_id,
a.revenue_count as revenue_count,
a.clicks_count as clicks_count,
a.impressions_count as impressions_count
FROM
	(SELECT
	a.eastern_date_sk,
	a.full_date,
	b.order_id,
	sum(a.revenue_count) as revenue_count,
	sum(a.clicks_count) as clicks_count,
	sum(a.impressions_count) as impressions_count
	FROM (SELECT
		  a.eastern_date_sk,
		  a.full_date,
		  b.flight_id,
		  sum(a.revenue) as revenue_count,
		  sum(a.clicks) as clicks_count,
		  sum(a.impressions) as impressions_count
		FROM
		  adm.daily_agg_adm_data_feed a,
		  refer.adm_creative_dim b
		WHERE
		  a.creative_id = b.creative_id AND
		  a.is_active = true AND
		  b.dt_expire = '9999-12-31' AND
		  a.eastern_date_sk = $P{eastern_date_sk}
		GROUP BY
		  a.eastern_date_sk,
		  a.full_date,
		  b.flight_id
		) a
	INNER JOIN
	refer.adm_flight_dim b ON a.flight_id=b.flight_id
	WHERE
	b.dt_expire = '9999-12-31'
	GROUP BY
	a.eastern_date_sk,
	a.full_date,
	b.order_id
	) a
INNER JOIN refer.adm_order_dim b ON a.order_id=b.order_id
WHERE
b.dt_expire = '9999-12-31'
ORDER BY
revenue_count desc
LIMIT 20
) a]]>
	</queryString>
	<field name="sum" class="java.lang.Double"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<pageHeader>
		<band height="20">
			<staticText>
				<reportElement x="0" y="0" width="200" height="20"/>
				<textElement verticalAlignment="Top">
					<font isBold="true"/>
				</textElement>
				<text><![CDATA[Yesterday Top 20 Revenues by Order:]]></text>
			</staticText>
			<textField pattern="###0.00;-###0.00">
				<reportElement x="200" y="0" width="64" height="20"/>
				<textElement verticalAlignment="Top">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.Double"><![CDATA[$F{sum}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement x="264" y="0" width="50" height="20"/>
				<textElement textAlignment="Right" verticalAlignment="Top">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[new DecimalFormat("0.00").format(new Double(($F{sum}.doubleValue()*100)/$P{totalRevenue})) + "%"]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<detail>
		<band height="29" splitType="Stretch">
			<subreport>
				<reportElement x="0" y="0" width="1250" height="29"/>
				<subreportParameter name="eastern_date_sk">
					<subreportParameterExpression><![CDATA[$P{eastern_date_sk}]]></subreportParameterExpression>
				</subreportParameter>
				<connectionExpression><![CDATA[$P{REPORT_CONNECTION}]]></connectionExpression>
				<subreportExpression class="java.lang.String"><![CDATA[$P{SUBREPORT_DIR} + "top_20_revenue_campaign_subreport2.jasper"]]></subreportExpression>
			</subreport>
		</band>
	</detail>
	<summary>
		<band height="30"/>
	</summary>
</jasperReport>
