	<queryString>
		<![CDATA[SELECT a.publisher_name,
sum(a.impressions) as impressions,
sum(a.clicks) as clicks,
sum(a.revenue)::Decimal as revenue,
sum(a.today_estimate_revenue) as today_estimate_revenue,
sum(a.p_rev_share)::Decimal as p_rev_share,
sum(a.api_revenue)::Decimal as api_revenue,
sum(a.v_rev_share)::Decimal as v_rev_share,
b.requests
FROM adm.daily_agg_network_performance a
LEFT JOIN
	(SELECT publisher_id,sum(requests) as requests FROM adm.daily_network_fct_request
	WHERE (property_id IN (select unnest(string_to_array($P{website_names_string}, ';'))::int) OR '-1000' = $P{website_names_string})
	AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date AND is_active=true
	GROUP BY publisher_id
	) b ON b.publisher_id=a.publisher_id
WHERE a.is_active=true
AND a.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND a.publisher_id=$P{p_publisher_id}::int
AND (a.property_id IN (select unnest(string_to_array($P{website_names_string}, ';'))::int) OR '-1000' = $P{website_names_string})
AND (a.network_id IN (select unnest(string_to_array($P{p_network_id}, ','))::int) OR '0' = $P{p_network_id})
GROUP BY a.publisher_name,b.requests]]>
	</queryString>
