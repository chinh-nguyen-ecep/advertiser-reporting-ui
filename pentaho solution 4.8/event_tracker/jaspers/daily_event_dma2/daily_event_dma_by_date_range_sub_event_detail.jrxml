	<queryString>
		<![CDATA[SELECT *
FROM (
SELECT
	flight_id,
	full_date,
	day_of_week,
	dma_id,
	dma_name,
	event_name,
	sum(event_count) as event_count
FROM evttracker.daily_event_dma
WHERE is_active=true
  AND (event_name IN (select unnest(string_to_array($P{event_names_string},';'))) OR 'All Event Names'=$P{event_names_string})
  AND flight_id IN (select unnest(string_to_array($P{flight_ids_string},';')))
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
GROUP BY flight_id,full_date, day_of_week, dma_id, dma_name, event_name
) a
ORDER BY flight_id,full_date,event_name,dma_name]]>
	</queryString>
