	<queryString>
		<![CDATA[SELECT *
FROM (
SELECT	full_date,
	hour24_of_day,
	day_of_week,
	dma_id,
	dma_name,
	event_name,
	sum(event_count) as event_count
FROM evttracker.daily_event_dma_by_hour
WHERE is_active=true
  AND event_name=$P{p_event_name}
  AND (flight_id=$P{p_flight_id} OR $P{p_flight_id} = 'All Flight ID')
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
GROUP BY full_date, hour24_of_day, day_of_week, dma_id, dma_name, event_name
) a
ORDER BY full_date, hour24_of_day, dma_name]]>
	</queryString>
