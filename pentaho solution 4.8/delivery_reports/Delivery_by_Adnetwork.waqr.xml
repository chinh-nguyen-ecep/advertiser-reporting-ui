<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE report PUBLIC "-//JFreeReport//DTD report definition//EN//simple/version 0.8.5" "http://jfreereport.sourceforge.net/report-085.dtd">

<report bottommargin="10" topmargin="10" rightmargin="10" leftmargin="10" pageformat="LETTER" orientation="landscape" name="Delivery_by_Adnetwork">
  <parser-config>
    <property name="reportfooter">
      <!-- label -->
    </property>
    <property name="pagefooter">
      <!-- label -->
    </property>
    <property name="reportheader">Delivery by Adnetwork</property>
    <property name="pageheader">Vervemobile.com</property>
  </parser-config>
  <reportheader color="#ff7d17" fontname="Arial" fontsize="16">
    <message-field alignment="left" height="18" vertical-alignment="middle" width="100%" x="0%" y="0">${reportheader}</message-field>
  </reportheader>
  <reportfooter color="#ff7d17" fontname="Arial" fontsize="16">
    <message-field alignment="center" height="18" vertical-alignment="top" width="100%" x="0%" y="0">${reportfooter}</message-field>
  </reportfooter>
  <pageheader color="#c0c0c0" fontname="Arial" fontsize="8">
    <message-field alignment="left" height="18" vertical-alignment="middle" width="50%" x="0%" y="0">${pageheader}</message-field>
    <message-field alignment="right" height="18" width="50%" x="50%" y="0">$(report.date, date, MMMM dd, yyyy @ hh:mm )</message-field>
    <line color="#9eaa36" height="3" weight="1" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
  </pageheader>
  <pagefooter color="#c0c0c0" fontname="Arial" fontsize="8">
    <line color="#9eaa36" height="3" weight="1" width="100%" x1="0%" x2="100%" y1="18" y2="18"/>
    <message-field alignment="left" height="18" width="50%" x="0" y="0">${pagefooter}</message-field>
    <message-field alignment="right" height="18" width="50%" x="50%" y="0">Page $(PageofPages)</message-field>
  </pagefooter>
  <functions>
    <property-ref name="report.date"/>
    <function class="org.jfree.report.function.PageOfPagesFunction" name="PageofPages"/>
    <function name="backgroundTrigger" class="org.jfree.report.function.ElementVisibilitySwitchFunction">
      <properties>
        <property name="element">rowBandingElement</property>
        <property name="numberOfElements">1</property>
        <property name="initialState">true</property>
      </properties>
    </function>
    <expression class="org.jfree.report.function.HideElementIfDataAvailableExpression" name="noDataLabelElementHideIfNullTrigger">
      <properties>
        <property name="element">noDataLabelElement</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="dummyGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">dummyGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ItemSumFunction" name="Summary_BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSESExpression">
      <properties>
        <property name="field">BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSES</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ItemSumFunction" name="Summary_BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSESExpression">
      <properties>
        <property name="field">BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSES</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="BC_DATE_DATEGroupHeaderHideIfNullTrigger">
      <properties>
        <property name="element">BC_DATE_DATEGroupHeader</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="BC_DATE_DATEGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">BC_DATE_DATEGroupFooter</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ItemSumFunction" name="BC_DATE_DATEBC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSESExpression">
      <properties>
        <property name="field">BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSES</property>
        <property name="group">BC_DATE_DATE</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ItemSumFunction" name="BC_DATE_DATEBC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSESExpression">
      <properties>
        <property name="field">BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSES</property>
        <property name="group">BC_DATE_DATE</property>
      </properties>
    </expression>
  </functions>
  <configuration>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowX">false</property>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.table.html.AssumeOverflowY">true</property>
  </configuration>
  <items color="#000000" fontname="Arial" fontsize="9" fontstyle="plain">
    <rectangle name="rowBandingElement" color="#e7e7e7" draw="false" fill="true" height="12" x="0%" width="100%" y="0"/>
    <date-field name="BC_DATE_DATEElement" nullstring="-" fieldname="BC_DATE_DATE" format="yyyy-MM-dd" vertical-alignment="middle" alignment="left" width="25.0%" x="0.0%" y="0" height="12" color="#000000" fontname="Arial" fontsize="9" fontstyle="plain"/>
    <string-field name="BC_AD_NETWORK_AD_NETWORK_NAMEElement" nullstring="-" fieldname="BC_AD_NETWORK_AD_NETWORK_NAME" vertical-alignment="middle" alignment="left" width="25.0%" x="25.0%" y="0" height="12" color="#000000" fontname="Arial" fontsize="10" fontstyle="plain"/>
    <number-field name="BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSESElement" nullstring="-" fieldname="BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSES" format="#,###" vertical-alignment="middle" alignment="right" width="25.0%" x="50.0%" y="0" height="12" color="#000000" fontname="Arial" fontsize="9" fontstyle="plain"/>
    <number-field name="BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSESElement" nullstring="-" fieldname="BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSES" format="#,###" vertical-alignment="middle" alignment="right" width="25.0%" x="75.0%" y="0" height="12" color="#000000" fontname="Arial" fontsize="9" fontstyle="plain"/>
    <line color="#c0c0c0" weight=".5" x1="0.0%" y1="0" x2="0.0%" y2="12"/>
    <line color="#c0c0c0" weight=".5" x1="25.0%" y1="0" x2="25.0%" y2="12"/>
    <line color="#c0c0c0" weight=".5" x1="50.0%" y1="0" x2="50.0%" y2="12"/>
    <line color="#c0c0c0" weight=".5" x1="75.0%" y1="0" x2="75.0%" y2="12"/>
    <line color="#c0c0c0" weight=".5" x1="100%" y1="0" x2="100%" y2="12"/>
    <line color="#c0c0c0" weight=".5" x1="0%" y1="0" x2="100%" y2="0"/>
    <line color="#c0c0c0" weight=".5" x1="0%" y1="12" x2="100%" y2="12"/>
  </items>
  <groups>
    <group name="dummy">
      <groupheader name="dummyGroupHeader" color="#767676" fontname="Arial" fontsize="12" fontstyle="bold" repeat="false">
        <label name="noDataLabelElement" height="12" vertical-alignment="middle" alignment="left" width="100%" x="0%" y="0" visible="false">No Data Available</label>
      </groupheader>
      <groupfooter name="dummyGroupFooter" color="#ff7d17" fontname="Arial" fontsize="12" fontstyle="bold">
        <rectangle color="#e0d0c0" draw="false" fill="true" height="17" width="100%" y="1" x="0%"/>
        <number-field fieldname="Summary_BC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSESExpression" height="16" vertical-alignment="middle" alignment="right" width="25.0%" x="50.0%" y="2" format="#,###"/>
        <number-field fieldname="Summary_BC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSESExpression" height="16" vertical-alignment="middle" alignment="right" width="25.0%" x="75.0%" y="2" format="#,###"/>
        <message-field height="16" x="0%" width="50.0%" y="2" alignment="left" vertical-alignment="middle">Grand Total</message-field>
      </groupfooter>
    </group>
    <group name="BC_DATE_DATE">
      <fields>
        <field>BC_DATE_DATE</field>
      </fields>
      <groupheader name="BC_DATE_DATEGroupHeader" repeat="false" pagebreak-before-print="false" pagebreak-after-print="false" color="#767676" fontname="Arial" fontsize="12" fontstyle="bold" height="38">
        <rectangle color="#FFFFFF" draw="false" fill="false" height="17" width="100%" x="0%" y="0"/>
        <message-field height="17" vertical-alignment="top" alignment="left" width="100%" x="0%" y="0" nullstring="-">Date: $(BC_DATE_DATE,date,yyyy-MM-dd)</message-field>
        <rectangle color="#9eaa36" draw="false" fill="true" height="18" width="100%" x="0%" y="19"/>
        <rectangle draw="false" fill="false" height="2" width="100%" x="0%" y="17"/>
        <label color="#ffffff" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="left" width="25.0%" x="0.0%" y="19">Date</label>
        <label color="#ffffff" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="left" width="25.0%" x="25.0%" y="19">Ad network name</label>
        <label color="#ffffff" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="right" width="25.0%" x="50.0%" y="19">Adcel Responses</label>
        <label color="#ffffff" fontname="Arial" fontsize="10" fontstyle="plain" height="18" vertical-alignment="middle" alignment="right" width="25.0%" x="75.0%" y="19">Yes Responses</label>
      </groupheader>
      <groupfooter name="BC_DATE_DATEGroupFooter" color="#ff7d17" fontname="Arial" fontsize="12" fontstyle="bold" pagebreak-before-print="false" pagebreak-after-print="false">
        <rectangle color="#c0d0e0" draw="false" fill="true" height="17" width="100%" y="1" x="0.0%"/>
        <number-field fieldname="BC_DATE_DATEBC_DAILY_AGG_DELIVERY_ADNETWORK_ADCEL_RESPONSESExpression" height="16" vertical-alignment="middle" alignment="right" width="25.0%" x="50.0%" y="2" format="#,###"/>
        <number-field fieldname="BC_DATE_DATEBC_DAILY_AGG_DELIVERY_ADNETWORK_YES_RESPONSESExpression" height="16" vertical-alignment="middle" alignment="right" width="25.0%" x="75.0%" y="2" format="#,###"/>
        <message-field height="16" x="0.0%" width="50.0%" y="2" alignment="left" vertical-alignment="middle">Total $(BC_DATE_DATE)</message-field>
      </groupfooter>
    </group>
  </groups>
</report>
