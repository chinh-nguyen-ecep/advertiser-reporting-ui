	<queryString>
		<![CDATA[SELECT a.*
FROM
(
--All partners
SELECT
	eastern_date_sk
	, full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, portal_id
	, portal_name
	, SUM(trxids_count) as total_imp
	, SUM(filled_count)::float/SUM(trxids_count) as fill_rate
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_0)::float/sum(trxids_count) end as na_no_score
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_1_48)::float/sum(trxids_count) end as quality_1_48
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_49_50)::float/sum(trxids_count) end as quality_49_50
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_51_100)::float/sum(trxids_count) end as quality_51_100
	, case sum(filled_count) when 0 then 0 else sum(country_us)::float/sum(filled_count) end as us
	, case sum(filled_count) when 0 then 0 else sum(country_international)::float/sum(filled_count) end as international
FROM adstraffic.daily_geo_quality
WHERE full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND is_active=true
AND '0'=$P{p_partner_id_list}
GROUP BY eastern_date_sk,full_date,publisher_id,partner_id,publisher_name,partner_name,portal_id, portal_name
UNION
--All publisher
SELECT
	eastern_date_sk
	, full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, portal_id
	, portal_name
	, SUM(trxids_count) as total_imp
	, SUM(filled_count)::float/SUM(trxids_count) as fill_rate
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_0)::float/sum(trxids_count) end as na_no_score
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_1_48)::float/sum(trxids_count) end as quality_1_48
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_49_50)::float/sum(trxids_count) end as quality_49_50
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_51_100)::float/sum(trxids_count) end as quality_51_100
	, case sum(filled_count) when 0 then 0 else sum(country_us)::float/sum(filled_count) end as us
	, case sum(filled_count) when 0 then 0 else sum(country_international)::float/sum(filled_count) end as international
FROM adstraffic.daily_geo_quality
WHERE full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND is_active=true
AND '0'=$P{p_publisher_id_list}
GROUP BY eastern_date_sk,full_date,publisher_id,partner_id,publisher_name,partner_name,portal_id, portal_name
--Not all publisher
UNION
SELECT
	eastern_date_sk
	, full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, portal_id
	, portal_name
	, SUM(trxids_count) as total_imp
	, SUM(filled_count)::float/SUM(trxids_count) as fill_rate
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_0)::float/sum(trxids_count) end as na_no_score
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_1_48)::float/sum(trxids_count) end as quality_1_48
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_49_50)::float/sum(trxids_count) end as quality_49_50
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_51_100)::float/sum(trxids_count) end as quality_51_100
	, case sum(filled_count) when 0 then 0 else sum(country_us)::float/sum(filled_count) end as us
	, case sum(filled_count) when 0 then 0 else sum(country_international)::float/sum(filled_count) end as international
FROM adstraffic.daily_geo_quality
WHERE full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND is_active=true
AND parent1_id IN (select unnest(string_to_array($P{p_publisher_id_list}, ';'))::int)
GROUP BY eastern_date_sk,full_date,publisher_id,partner_id,publisher_name,partner_name,portal_id, portal_name
--Not all partner
UNION
SELECT
	eastern_date_sk
	, full_date
	, parent1_id as publisher_id
	, parent1_name as publisher_name
	, partner_id
	, partner_name
	, portal_id
	, portal_name
	, SUM(trxids_count) as total_imp
	, SUM(filled_count)::float/SUM(trxids_count) as fill_rate
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_0)::float/sum(trxids_count) end as na_no_score
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_1_48)::float/sum(trxids_count) end as quality_1_48
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_49_50)::float/sum(trxids_count) end as quality_49_50
	, case sum(trxids_count) when 0 then 0 else sum(location_quality_51_100)::float/sum(trxids_count) end as quality_51_100
	, case sum(filled_count) when 0 then 0 else sum(country_us)::float/sum(filled_count) end as us
	, case sum(filled_count) when 0 then 0 else sum(country_international)::float/sum(filled_count) end as international
FROM adstraffic.daily_geo_quality
WHERE full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND is_active=true
AND partner_id IN (select unnest(string_to_array($P{p_partner_id_list}, ';'))::int)
GROUP BY eastern_date_sk,full_date,publisher_id,partner_id,publisher_name,partner_name,portal_id, portal_name
)a
ORDER BY eastern_date_sk,publisher_name,partner_name, portal_name]]>
