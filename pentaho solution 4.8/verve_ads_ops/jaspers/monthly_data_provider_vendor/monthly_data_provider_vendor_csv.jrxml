	<queryString>
		<![CDATA[SELECT
calendar_year_month as "Month"
,order_id as "ADM Order Id"
,order_name as "ADM Order Name"
,flight_id as "ADM Flight Id"
,flight_name as "ADM Flight Name"
,flight_start_date as "ADM Flight Start Date"
,flight_end_date as "ADM Flight End Date"
--,flight_quantity as booked_units
--,flight_metric as metric
--,Case network_id when 1 THEN 'Local' WHEN 4 THEN 'VLM' END as "Network"
,vendor_id as "Vendor id"
,vendor_name as "Vendor Name"
,vendor_role as "Vendor Role"
,max(dt_lastchange) as dt_lastchange
FROM adsops.monthly_agg_data_provider_vendor
WHERE is_active=true AND month_since_2005=$P{p_month_since_2005}::int
GROUP BY
calendar_year_month
,month_since_2005
,order_id
,order_name
,flight_id
,flight_name
,flight_start_date
,flight_end_date
,vendor_id
,vendor_name
,vendor_role
ORDER BY
order_name
,flight_name
,vendor_name
,vendor_role]]>
	</queryString>
