	<parameter name="p_list_adnetwork_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
a.full_date
,b.RemnantRequests
,a.VerveAdsRequests
,a.VerveAdsFills
,a.Errors
,a.Timeouts
,(case WHEN b.RemnantRequests>0 THEN a.VerveAdsRequests::double precision/b.RemnantRequests else 0 END) as Coverage
,(case WHEN a.VerveAdsRequests>0 THEN a.VerveAdsFills::double precision/a.VerveAdsRequests else 0 END) as fill
,(case WHEN a.VerveAdsRequests>0 THEN a.Errors::double precision/a.VerveAdsRequests else 0 END) as error
,(case WHEN a.VerveAdsRequests>0 THEN a.Timeouts::double precision/a.VerveAdsRequests else 0 END) as timeout
FROM
(
SELECT
full_date
,sum(fullfilled_code_count) as VerveAdsRequests
,sum(fullfilled_code_y) as VerveAdsFills
,sum(fullfilled_code_e) as Errors
,sum(fullfilled_code_t) as Timeouts
FROM adstraffic.daily_ad_serving_stats
WHERE is_active=true
AND ad_network_id IN (select unnest(string_to_array($P{p_list_adnetwork_id}::text,';'))::int)
GROUP BY full_date
) a
LEFT JOIN
(
SELECT
a.full_date
,max(b.total_ad_requests)-sum(a.fullfilled_code_y) as RemnantRequests
FROM adstraffic.daily_ad_serving_stats a,adstraffic.daily_adcel_stats b
WHERE a.is_active=true AND b.is_active=true
AND a.full_date=b.full_date
AND a.ad_network_id=10
GROUP BY a.full_date
)  b ON a.full_date=b.full_date
WHERE a.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
ORDER BY a.full_date]]>
	</queryString>
