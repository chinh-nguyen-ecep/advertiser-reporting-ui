,vendor_names as "Vendor Names"
,vendor_roles as "Vendor Roles"
,max(dt_lastchange) as dt_lastchange
FROM adsops.weekly_agg_flight_missing_vendor
WHERE is_active=true AND year_week=$P{p_year_week}
GROUP BY
start_week
,end_week
,order_id
,order_name
,flight_id
,flight_name
,flight_start_date
,flight_end_date
,vendor_ids
,vendor_names
,vendor_roles
ORDER BY
order_name
,flight_name
,vendor_names
,vendor_roles]]>
