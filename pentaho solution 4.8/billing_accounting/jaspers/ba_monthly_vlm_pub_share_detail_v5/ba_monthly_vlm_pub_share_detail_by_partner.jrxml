	<queryString>
		<![CDATA[SELECT a.partner_id
, b.name_current as partner_name
, p_revenue_share
, sum(delivered_units) as delivered_units
, sum(billable_units) as billable_units
, sum(gross_revenue) as gross_revenue
, sum(billable_revenue) as billable_revenue
, sum(p_net_revenue) as p_net_revenue
FROM (
SELECT partner_id
, ROUND((partner_revenue_share::float/100)::NUMERIC, 2) as p_revenue_share
, delivered_units
, partner_billable_units as billable_units
, gross_revenue
, billable_revenue
, p_net_revenue
FROM billing.ba_monthly_vlm_revenue_v5 a
WHERE calendar_year_month = $P{calendar_year_month}::text
AND delivered_units > 0
AND is_active = true
) a
INNER JOIN refer.partner_dim b ON b.dt_expire = '9999-12-31' AND b.partner_id = a.partner_id
--WHERE p_revenue_share > 0
GROUP BY a.partner_id
, b.name_current
, a.p_revenue_share
ORDER BY 2,3]]>
	</queryString>
