sum(impressions) as paid_impressions,
sum(case clicks when -1 then 0 else clicks end) as clicks,
sum(revenue) as net_revenue,
sum(fullfilled_code_count) as total_adcel_total,
sum(fullfilled_code_y) as total_adcel_yes
