        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Make OLAP query</action-type>
        <action-inputs> 
          <p_measures type="string"/>  
          <t_measures type="string"/>  
          <p_date_mode type="string"/>  
          <v_user_choose_mode type="string"/>  
          <queryOLAP type="string"/>  
          <v_chart_sub_title type="string"/>  
          <v_chart_title type="string"/>  
          <p_report_type type="string"/>  
          <t_date_list type="string"/>  
          <t_month_list type="string"/>  
          <t_property type="string"/>  
          <t_publisher type="string"/>  
          <p_Property type="string"/>  
          <p_Publisher type="string"/> 
        </action-inputs>
        <action-outputs> 
          <queryOLAP type="string"/>  
          <v_chart_title type="string"/>  
          <v_chart_sub_title type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[v_chart_title=p_report_type;
if(v_user_choose_mode=='multiDimensions'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {'+t_property+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {'+t_property+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	};
	// set chart title
	v_chart_sub_title=p_measures +' on ' + p_Publisher;
}else if(v_user_choose_mode=='multiMeasures'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({'+t_property+'}, {'+t_measures+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({'+t_property+'}, {'+t_measures+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	};
	// set chart title
	v_chart_sub_title=p_Property+' on ' + p_Publisher;
}else if(v_user_choose_mode ==  'multiDimensionMeasures'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {'+t_property+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {'+t_property+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_publisher+'})';
	};
	// set chart title
	v_chart_sub_title=p_measures[0] +' on ' + p_Publisher;
}]]></script> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <action-definition> 
      <component-name>MDXLookupRule</component-name>
      <action-type>OLAP</action-type>
      <action-inputs> 
        <query type="string" mapping="queryOLAP"/> 
      </action-inputs>
      <action-resources> 
        <catalog mapping="catalog1"/> 
      </action-resources>
      <action-outputs> 
        <query-results type="result-set" mapping="OLAPResult"/> 
      </action-outputs>
      <component-definition> 
        <jndi><![CDATA[verveReportConnection]]></jndi>  
        <location><![CDATA[mondrian]]></location> 
      </component-definition> 
    </action-definition>
  
    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Create HTMLdata</action-type>
      <action-inputs> 
        <OLAPResult type="result-set"/>  
        <p_measures type="string"/>  
        <chartName type="string"/> 
      </action-inputs>
      <action-outputs> 
        <HTMLTable type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var results;
var HTMLTable;
var measures;
results = OLAPResult;
HTMLTable='';
measures=typeof(p_measures);
var meta = results.getMetaData();      // Returns an IPentahoMetaData object
var colCount = meta.getColumnCount();
var colHeaders = meta.getColumnHeaders(); //getColumnHeaders() returns object[][]
var rowHeaders = meta.getRowHeaders(); 
HTMLTable+='<table id="'+chartName+'_datatable" class="datatable"><thead><tr><th></th>';
for ( var i = 0 ; i < colCount ; i++ ){
   
	HTMLTable+='<th width="80">'+colHeaders[1][i]+'</th>';

}
HTMLTable+='</tr></thead><tbody>';



for(var i=0;i<results.getRowCount();i++){
	if(i%2==0){
	HTMLTable+='<tr>';
	}else{
	HTMLTable+='<tr class="tr1">';
	}
	
	HTMLTable+='<th>'+rowHeaders[i][0]+'</th>';
	for(var j=0;j<results.getColumnCount();j++){ 
		var temp=results.getValueAt(i,j);
		
		if(temp==null) temp=0;
		temp=temp/1;
		if(measures == 'string' && p_measures == 'Revenue'){
			temp=temp.toFixed(2);
		}else
		if(colHeaders[1][j]=='Revenue'){
			temp=temp.toFixed(2);
		}
		
		HTMLTable+='<td>'+temp+'</td>';
	}
	HTMLTable+='</tr>'
}

HTMLTable+='</tbody></table>';]]></script> 
      </component-definition> 
    </action-definition>
  
