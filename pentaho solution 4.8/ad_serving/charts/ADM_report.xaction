    <action-definition> 
      <component-name>JavascriptRule</component-name>
      <action-type>Convert Parameter to OLAP script</action-type>
      <action-inputs> 
        <p_Campaign type="string-list"/>  
        <p_Site type="string"/>  
        <q_calendar_year_month type="result-set"/>  
        <q_date_list type="result-set"/>  
        <p_start_month type="string"/>  
        <p_end_month type="string"/>  
        <p_measures type="string"/>  
        <p_Publisher type="string"/>  
        <p_Placement type="string"/>  
        <p_Creative type="string"/>  
        <p_Property type="string"/> 
      </action-inputs>
      <action-outputs> 
        <t_measures type="string"/>  
        <t_campain type="string"/>  
        <t_site type="string"/>  
        <t_date_list type="string"/>  
        <t_month_list type="string"/>  
        <t_publisher type="string"/>  
        <t_placement type="string"/>  
        <t_creative type="string"/>  
        <t_property type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var t_measures;
var t_campain;
var t_date_list;
var t_month_list;
var t_site;
var t_publisher;
var t_placement;
var t_creative;
var t_property;
//set default value
t_measures='';
t_campain='';
t_date_list='';
t_month_list='';
t_site='';
t_publisher='';
t_placement='';
t_creative='';
t_property='';

//convert measures
if(p_measures!= ''){
	if(p_measures instanceof Array){
		for(var i=0;i<p_measures.length;i++){
			if(i>0){
			t_measures+=',';
			};
			t_measures+='[Measures].['+p_measures[i]+']';
		};
	}else{
		t_measures+='[Measures].['+p_measures+']';
	};
}


//convert dimension
if(p_Campaign != ''){
	if(p_Campaign instanceof Array){
		for(var i=0;i<p_Campaign.length;i++){
			if(i>0){
			t_campain+=',';
			};
			t_campain+='[Order].['+p_Campaign[i].replace(']',']]')+']';
		};
	}else{
		t_campain+='[Order].['+p_Campaign.replace(']',']]')+']';
	};
}


//convert site
if(p_Site !=''){
	if(p_Site instanceof Array){
		for(var i=0;i<p_Site.length;i++){
			if(i>0){
			t_site+=',';
			};		
			t_site+='[Site].['+p_Site[i].replace(']',']]')+']';
		};
	}else{
		t_site+='[Site].['+p_Site.replace(']',']]')+']';
	};
}


//convert publisher
if(p_Publisher !=''){
	if(p_Publisher instanceof Array){
		for(var i=0;i<p_Publisher.length;i++){
			if(i>0){
			t_publisher+=',';
			};		
			t_publisher+='[Publisher].['+p_Publisher[i].replace(']',']]')+']';
		};
	}else{
		t_publisher+='[Publisher].['+p_Publisher.replace(']',']]')+']';
	};
};

//convert property
if(p_Property !=''){
	if(p_Property instanceof Array){
		for(var i=0;i<p_Property.length;i++){
			if(i>0){
			t_property+=',';
			};		
			t_property+='[Property].['+p_Property[i].replace(']',']]')+']';
		};
	}else{
		t_property+='[Property].['+p_Property.replace(']',']]')+']';
	};
};

//convert placement
if(p_Placement !=''){
	if(p_Placement instanceof Array){
		for(var i=0;i<p_Placement.length;i++){
			if(i>0){
			t_placement+=',';
			};		
			t_placement+='[Placement].['+p_Placement[i].replace(']',']]')+']';
		};
	}else{
		t_placement+='[Placement].['+p_Placement.replace(']',']]')+']';
	};
};


//convert placement
if(p_Creative !=''){
	if(p_Creative instanceof Array){
		for(var i=0;i<p_Creative.length;i++){
			if(i>0){
			t_creative+=',';
			};		
			t_creative+='[Creative].['+p_Creative[i].replace(']',']]')+']';
		};
	}else{
		t_creative+='[Creative].['+p_Creative.replace(']',']]')+']';
	};
};

//convert date list
var countDateList=q_date_list.getRowCount();
for(var i=0;i<countDateList;i++){
		if(i>0){
			t_date_list+=',';
		};
		t_date_list+='[Date].['+q_date_list.getValueAt(i,0)+']';		
	};

//convert month list
var minRage;
var maxRage;
if(p_start_month<p_end_month){
	minRage=p_start_month;
	maxRage=p_end_month;
}else{
	minRage=p_end_month;
	maxRage=p_start_month;
};

for(var i=maxRage;i>=minRage;i--){
		if(i<maxRage){
			t_month_list+=',';
		};
		t_month_list+='[Year month].['+q_calendar_year_month.getValueAt(i,1)+']';
				
	};]]></script> 
      </component-definition> 
    </action-definition>
  
    <actions> 
      <condition><![CDATA[p_report_type=='Placement daily report']]></condition>  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Get user choose mode</action-type>
        <action-inputs> 
          <p_measures type="string"/>  
          <p_Creative type="string"/>  
          <v_user_choose_mode type="string"/> 
        </action-inputs>
        <action-outputs> 
          <v_user_choose_mode type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var measures;
var dimension;

measures=typeof(p_measures);
dimension=typeof(p_Creative);

if( measures == 'string' && dimension == 'object'){
	v_user_choose_mode='multiDimensions';
}else if( measures == 'object' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'string' && dimension == 'string'){
	v_user_choose_mode = 'multiMeasures';
}else if( measures == 'object' && dimension == 'object'){
	v_user_choose_mode = 'multiDimensionMeasures';
}]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Make OLAP query</action-type>
        <action-inputs> 
          <p_measures type="string"/>  
          <t_creative type="string"/>  
          <t_measures type="string"/>  
          <t_placement type="string"/>  
          <p_Placement type="string"/>  
          <p_Creative type="string"/>  
          <p_date_mode type="string"/>  
          <v_user_choose_mode type="string"/>  
          <queryOLAP type="string"/>  
          <v_chart_sub_title type="string"/>  
          <v_chart_title type="string"/>  
          <p_report_type type="string"/>  
          <t_date_list type="string"/>  
          <t_month_list type="string"/> 
        </action-inputs>
        <action-outputs> 
          <queryOLAP type="string"/>  
          <v_chart_title type="string"/>  
          <v_chart_sub_title type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[v_chart_title=p_report_type;
if(v_user_choose_mode=='multiDimensions'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {'+t_creative+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {'+t_creative+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	};
	// set chart title
	v_chart_sub_title=p_measures +' on ' + p_Placement;
}else if(v_user_choose_mode=='multiMeasures'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({'+t_creative+'}, {'+t_measures+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({'+t_creative+'}, {'+t_measures+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	};
	// set chart title
	v_chart_sub_title=p_Creative+' on ' + p_Placement;
}else if(v_user_choose_mode ==  'multiDimensionMeasures'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {'+t_creative+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	}else if(p_date_mode=='month'){
	queryOLAP='select Crossjoin({[Measures].['+p_measures[0]+']}, {'+t_creative+'}) ON COLUMNS, {'+t_month_list+'} ON ROWS from [daily_agg_network_performance] where Crossjoin({[Active].[true]}, {'+t_placement+'})'
	};
	// set chart title
	v_chart_sub_title=p_measures[0] +' on ' + p_Placement;
}]]></script> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
