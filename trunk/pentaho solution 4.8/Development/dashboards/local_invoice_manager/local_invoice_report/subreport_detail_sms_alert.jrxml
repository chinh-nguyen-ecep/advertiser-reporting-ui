		<![CDATA[select
inv_detail_id
,sms_description as "Description"
,sms_quantity as "Quantity"
,sms_rate as "Rate"
,sms_fee as sms_fee
from accounting.ba_inv_local_report_detail
where inv_mmyy=$P{p_month_year} --pass month
and status = 'ACTIVE' --pass status
and inv_detail_id = $P{p_inv_detail_id} --pass specific invoice number
and sms_fee>0
and account_partner_map_id=$P{account_partner_map_id}
and adm_partner_id = $P{p_adm_partner_id}
group by
inv_detail_id
,sms_description
,sms_quantity
,sms_rate
,sms_fee
order by sms_fee;]]>
