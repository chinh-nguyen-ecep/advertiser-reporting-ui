	<parameter name="total_event" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT *
FROM (
SELECT	full_date,day_of_week,
	sum(event_count) as event_count
FROM evttracker.daily_event_by_distance
WHERE flight_id=$P{p_flight_id}
  AND is_active=true
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
  AND event_name IN (select unnest(string_to_array($P{event_names_string},',')))
GROUP BY full_date,day_of_week
UNION
SELECT	full_date,day_of_week,
	sum(event_count) as event_count
FROM evttracker.daily_event_by_distance
WHERE $P{p_flight_id}='All Flight ID'
  AND is_active=true
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
  AND event_name IN (select unnest(string_to_array($P{event_names_string},',')))
GROUP BY full_date,day_of_week
) a
ORDER BY full_date]]>
	</queryString>
