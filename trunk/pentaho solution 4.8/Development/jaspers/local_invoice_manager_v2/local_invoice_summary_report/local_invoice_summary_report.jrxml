	<queryString>
		<![CDATA[SELECT
a.INVOICE  as "Invoice"
,a.ACCOUNT_NAME as "Account Name"
,SUM(TOTAL_IMPS) AS "Total Impression"
,SUM(GROSS_LOCAL_REVENUE)  AS "Gross Local Revenue"
,SUM(PLATFORM_FEE)  AS "Platform Fee"
,SUM(SMS_FEE) AS "Sms fee"
,SUM(VERVE_LOCAL_NET)  AS "Verve Local Split"
,COALESCE(verve_global_fee,0) AS "Verve Global fee"
,COALESCE(verve_invoice_amount,0) AS "Verve Invoice Amount"
FROM(
	SELECT
	inv_detail_id as INVOICE
	,ACCOUNT_NAME
	,adm_partner_id
	,SUM(COALESCE(BILLABLE_UNITS,0)) AS TOTAL_IMPS
	,SUM(COALESCE(GROSS_REVENUE,0)) AS GROSS_LOCAL_REVENUE
	,COALESCE(PLATFORM_FEE,0) as platform_fee
	,COALESCE(SMS_FEE,0) as sms_fee
	,COALESCE(LOCAL_SUBTOTAL,0) as VERVE_LOCAL_NET
	,coalesce(global_fee) as VERVE_GLOBAL_FEE
	,COALESCE(invoice_amount, 0) as verve_invoice_amount
	FROM accounting.ba_inv_Local_report_detail
	WHERE INV_MMYY=  $P{p_month_year}--pass month
	and status = 'ACTIVE' --pass active status
	--and inv_detail_id = '0813004'
	GROUP BY inv_detail_id
	,ACCOUNT_NAME
	,adm_partner_id
	,PLATFORM_FEE
	,SMS_FEE
	,LOCAL_SUBTOTAL
	,global_fee
	,invoice_amount
)a
GROUP BY a.INVOICE
,a.ACCOUNT_NAME
,a.verve_invoice_amount
,a.verve_global_fee
ORDER BY ACCOUNT_NAME;]]>
	</queryString>
