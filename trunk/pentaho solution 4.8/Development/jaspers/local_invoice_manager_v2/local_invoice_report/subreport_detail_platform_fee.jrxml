	<field name="account_partner_map_id" class="java.lang.Integer"/>
	<variable name="platform_fee_1" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{platform_fee}]]></variableExpression>
	</variable>
	<variable name="local_sub_total" class="java.lang.Double" resetType="None">
		<variableExpression><![CDATA[( ($P{local_total}-$V{platform_fee_1})<0 ? $V{platform_fee_1} : (($P{local_total}-$V{platform_fee_1})*$P{verve_share})+$V{platform_fee_1} )]]></variableExpression>
