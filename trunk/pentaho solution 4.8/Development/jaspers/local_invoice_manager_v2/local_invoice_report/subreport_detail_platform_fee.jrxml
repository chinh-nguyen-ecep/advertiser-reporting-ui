	<queryString>
		<![CDATA[SELECT
a.platform_name AS type
,a.monthly_fee AS Platform_fee
,a.account_partner_map_id
FROM(
SELECT
c.inv_detail_id
,c.account_partner_map_id
,b.name AS platform_name
,a.monthly_fee
FROM  accounting.ba_platform_fee a
INNER JOIN accounting.ba_platform b
ON(b.is_active = 1 AND b.id = a.platform_id)
INNER JOIN( SELECT distinct inv_detail_id, account_partner_map_id
FROM accounting.ba_inv_local_report_detail_v2
WHERE
inv_mmyy = $P{p_month_year}
AND status = 'ACTIVE'
)c
ON(c.account_partner_map_id = a.account_partner_map_id)
WHERE
a.is_active = 1
AND a.decommission_date='9999-12-31'
)a
WHERE
a.inv_detail_id = $P{p_inv_detail_id}
AND a.account_partner_map_id =  $P{account_partner_map_id}]]>
	</queryString>
	<field name="type" class="java.lang.String"/>
