	<queryString>
		<![CDATA[SELECT
sum(revenue) as total_revenue_today,
full_date,
sum(budget) as total_budget,
min(start_date) as min_start_date,
max(end_date) as max_end_date,
sum(cumulative_revenue+revenue) as revenue_total_spent,
sum(budget)-(sum(cumulative_revenue+revenue)) as rest_of_budget,
sum(mtd_total_revenue) as mtd_total_revenue,
sum(remaining_monthly_budget) as remaining_monthly_budget,
sum(next_month_budget) as next_month_budget
FROM adm.daily_agg_revenue_by_order
WHERE eastern_date_sk=$P{eastern_date_sk}::int
AND is_active=true
GROUP BY
full_date]]>
