	<subDataset name="7DayReportByHour">
		<queryString>
			<![CDATA[SELECT
hour,
day AS date,
count(*) AS total
FROM stage.fact_event_tracker_agg_by_hour_hourly
WHERE day BETWEEN current_date -7 AND current_date -1 GROUP BY day, hour
ORDER BY date,hour]]>
		</queryString>
		<field name="hour" class="java.lang.Integer"/>
		<field name="date" class="java.sql.Date"/>
		<field name="total" class="java.lang.Long"/>
	</subDataset>
	<subDataset name="7DayBasicAggByEventType">
		<queryString>
			<![CDATA[select
event_type,
date, count(*) as total
from stage.fact_event_basic_agg_v5_1
where date BETWEEN current_date -7 AND current_date -1
group by date, event_type
order by date, event_type]]>
		</queryString>
		<field name="event_type" class="java.lang.String"/>
		<field name="date" class="java.sql.Date"/>
		<field name="total" class="java.lang.Long"/>
	</subDataset>
	<subDataset name="7DayEventAnalyticsByGroup">
		<queryString>
			<![CDATA[select
group_type,
date,  count(*) as total
from stage.fact_event_analytics_agg_v5_1
where date BETWEEN current_date -7 AND current_date -1
group by date, group_type
order by date, group_type]]>
		</queryString>
		<field name="group_type" class="java.lang.String"/>
		<field name="date" class="java.sql.Date"/>
		<field name="total" class="java.lang.Long"/>
	</subDataset>
