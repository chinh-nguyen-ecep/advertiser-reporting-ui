	<queryString>
		<![CDATA[SELECT full_date,network_name,
sum(impressions) as impressions,
sum(clicks) as clicks,
sum(revenue)::Decimal as revenue,
sum(p_rev_share)::Decimal as p_rev_share,
sum(v_rev_share)::Decimal as v_rev_share,
sum(today_ys) as today_ys,
sum(today_estimate_revenue) as today_estimate_revenue,
sum(total_paid_impressions) as total_paid_impressions,
sum(total_net_revenue) as total_net_revenue,
sum(total_p_rev_share) as total_p_rev_share,
sum(api_revenue) as api_revenue,
sum(total_v_rev_share) as total_v_rev_share
FROM adm.daily_agg_network_performance a
WHERE is_active=true
AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
AND publisher_id=$P{p_publisher_id}::int
AND (property_id IN (select unnest(string_to_array($P{website_names_string}, ';'))::int)
OR '-1000'= $P{website_names_string})
AND (a.network_id IN (select unnest(string_to_array($P{p_network_id}, ','))::int) OR '0' = $P{p_network_id})
GROUP BY full_date,network_name
ORDER BY full_date,network_name]]>
	</queryString>
