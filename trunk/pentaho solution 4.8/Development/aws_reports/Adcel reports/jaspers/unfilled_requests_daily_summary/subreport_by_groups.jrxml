<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subreport_by_groups" pageWidth="802" pageHeight="555" orientation="Landscape" columnWidth="802" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="eastern_date_sk" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer("1")]]></defaultValueExpression>
	</parameter>
	<parameter name="total_unfilled_count" class="java.lang.Long">
		<defaultValueExpression><![CDATA[new Long("1")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
a.eastern_date_sk,
a.parent1_id,
a.parent1_name,
sum(unfilled_count) as unfilled_count
FROM adstraffic.daily_unfilled_stats a
WHERE a.eastern_date_sk = $P{eastern_date_sk}
AND a.is_active = true
GROUP BY
a.eastern_date_sk,
a.parent1_id,
a.parent1_name
ORDER BY
unfilled_count DESC]]>
	</queryString>
	<field name="eastern_date_sk" class="java.lang.Integer"/>
	<field name="parent1_id" class="java.lang.Integer"/>
	<field name="parent1_name" class="java.lang.String"/>
	<field name="unfilled_count" class="java.lang.Long"/>
	<background>
		<band/>
	</background>
	<title>
		<band height="68">
			<staticText>
				<reportElement x="0" y="35" width="200" height="30"/>
				<textElement>
					<font fontName="Arial" size="10" isBold="false"/>
				</textElement>
				<text><![CDATA[Group Name (ID)]]></text>
			</staticText>
			<staticText>
				<reportElement x="200" y="35" width="70" height="30"/>
				<textElement textAlignment="Right">
					<font fontName="Arial" size="10" isBold="false"/>
				</textElement>
				<text><![CDATA[Unfilled Requests]]></text>
			</staticText>
			<line>
				<reportElement x="0" y="67" width="802" height="1"/>
			</line>
			<staticText>
				<reportElement x="270" y="35" width="50" height="30"/>
				<textElement textAlignment="Center">
					<font fontName="Arial" size="10" isBold="false"/>
				</textElement>
				<text><![CDATA[%]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="0" width="802" height="35"/>
				<textElement verticalAlignment="Middle">
					<font fontName="Arial" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[Unfilled Requests By Groups:]]></text>
			</staticText>
		</band>
	</title>
	<pageHeader>
		<band/>
	</pageHeader>
	<detail>
		<band height="21">
			<textField>
				<reportElement x="0" y="1" width="200" height="20"/>
				<textElement>
					<font fontName="Arial"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{parent1_name} + " (" + $F{parent1_id} + ")"]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="200" y="1" width="70" height="20"/>
				<textElement textAlignment="Right">
					<font fontName="Arial"/>
				</textElement>
				<textFieldExpression class="java.lang.Long"><![CDATA[$F{unfilled_count}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="270" y="1" width="50" height="20"/>
				<textElement textAlignment="Right">
					<font fontName="Arial"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[new DecimalFormat("0.00").format(new Double(($F{unfilled_count}.doubleValue()*100)/$P{total_unfilled_count}.doubleValue())) + "%"]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<columnFooter>
		<band height="22">
			<textField>
				<reportElement x="200" y="2" width="70" height="20"/>
				<textElement textAlignment="Right">
					<font fontName="Arial"/>
				</textElement>
				<textFieldExpression class="java.lang.Long"><![CDATA[$P{total_unfilled_count}]]></textFieldExpression>
			</textField>
			<line>
				<reportElement x="0" y="1" width="802" height="1"/>
			</line>
		</band>
	</columnFooter>
	<summary>
		<band/>
	</summary>
</jasperReport>
