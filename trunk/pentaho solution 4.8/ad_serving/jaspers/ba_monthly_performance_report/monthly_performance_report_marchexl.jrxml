	<queryString>
		<![CDATA[SELECT calendar_year_month,
(max (a.dt_lastchange)::timestamp with time zone AT TIME ZONE 'America/Los_Angeles')::timestamp with time zone AT TIME ZONE 'America/New_York'as dt_lastchange,
sum(total_ad_requests) as requests,
sum(paid_impressions) as paid_impressions,
sum(clicks) as clicks,
sum("cost") as net_revenue,
sum(gross_revenue) as gross_revenue,
sum(p_net_revenue) as p_net_revenue,
sum(v_net_revenue) as v_net_revenue,
sum(case fullfilled_code_count when -1 then 0 else fullfilled_code_count end) as total_adcel_total,
sum(case fullfilled_code_y when -1 then 0 else fullfilled_code_y end) as total_adcel_yes
  FROM adnetwork.ba_monthly_mx_performance a
  WHERE calendar_year_month=$P{calendar_year_month} AND is_active=true
  GROUP BY calendar_year_month]]>
	</queryString>
