	<parameter name="parameter1" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/opt/pentaho/biserver-ce/pentaho-solutions/third_party_reports/jaspers/daily_performance_report/"]]></defaultValueExpression>
	</parameter>
	<parameter name="calendar_year_month" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT calendar_year_month
     , MAX(dt_lastchange) AS dt_lastchange
     , SUM(imps)::bigint AS imps
     , SUM(gross_reveue) AS gross_reveue
     , SUM(pub_net_revenue) AS pub_net_revenue
FROM (
SELECT calendar_year_month
     , network_title
     , merchant_id
     , merchant_name
     , parent1_id
     , parent1_name
     , partner_id
     , partner_name
     , partner_keyword
     , SUM(imps)::bigint AS imps
     , billable_rate::float
     , partner_share
     , billable_rate * (SUM(imps)::float / 1000) AS gross_reveue
     , (billable_rate *(SUM(imps)::float / 1000)) * (partner_share::double precision / 100) AS pub_net_revenue
     , (max(dt_lastchange)::timestamp with time zone AT TIME ZONE 'America/Los_Angeles')::timestamp with time zone AT TIME ZONE 'America/New_York' AS dt_lastchange
FROM billing.ba_monthly_vlmo_publisher_share
WHERE calendar_year_month = $P{calendar_year_month}::text
GROUP BY calendar_year_month
       , network_title
       , merchant_id
       , merchant_name
       , parent1_id
       , parent1_name
       , partner_id
       , partner_name
       , partner_keyword
       , billable_rate
       , partner_share
) a
GROUP BY calendar_year_month

]]>
	</queryString>
