<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subreport_device_organic" pageWidth="555" pageHeight="802" columnWidth="535" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<parameter name="eastern_date_sk" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="parent1_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="total_page_views" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.eastern_date_sk,
	a.parent1_id,
	a.device_type,
	SUM(a.page_view_count)::int as page_view_count
FROM dw.daily_agg_partner_act_device a
WHERE
     a.eastern_date_sk = $P{eastern_date_sk}::int
 AND a.parent1_id = $P{parent1_id}::int
 AND a.organic = true
 AND a.is_active = true
GROUP BY a.eastern_date_sk,
	a.parent1_id,
	a.device_type
ORDER BY page_view_count desc]]>
	</queryString>
	<field name="eastern_date_sk" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="parent1_id" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="device_type" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="page_view_count" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<background>
		<band/>
	</background>
	<title>
		<band height="55">
			<line>
				<reportElement positionType="FixRelativeToBottom" x="0" y="50" width="555" height="1"/>
			</line>
			<staticText>
				<reportElement x="0" y="20" width="153" height="20"/>
				<textElement>
					<font fontName="Arial" size="14" isBold="true"/>
				</textElement>
				<text><![CDATA[Pageviews by Device (organic):]]></text>
			</staticText>
		</band>
	</title>
	<detail>
		<band height="20">
			<textField>
				<reportElement x="200" y="0" width="100" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{page_view_count}]]></textFieldExpression>
			</textField>
			<textField pattern="#,##0.00 %">
				<reportElement x="300" y="0" width="100" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.Double"><![CDATA[new Double(  ($F{page_view_count}.doubleValue()) /($P{total_page_views}.doubleValue()) )]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="0" y="0" width="200" height="20"/>
				<textElement>
					<font fontName="Arial" size="12"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{device_type}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
