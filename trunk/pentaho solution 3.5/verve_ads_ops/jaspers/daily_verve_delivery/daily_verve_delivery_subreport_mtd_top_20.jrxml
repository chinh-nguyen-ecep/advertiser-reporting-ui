	</parameter>
	<queryString>
		<![CDATA[SELECT
full_date AS "Date"
,calendar_year_month
,ROW_NUMBER() OVER (ORDER BY national_impressions DESC) AS RN
,platform
,publisher_id
,publisher_name
,national_impressions
,vlm_impressions
,vlmo_impressions
,remnant_impressions AS "Verve Ads imps"
,vpc_impressions AS "VPC imp"
,national_revenue
,vlm_revenue
,vpc_revenue
,vlmo_revenue
,remnant_revenue
,total_revenue
,total_net_revenue
,total_clicks
,CASE WHEN total_impressions>0 then total_clicks::float/total_impressions  ELSE 0 END CTR_rate
,total_impressions
,total_requests
,CASE WHEN total_requests>0 THEN total_impressions::float/total_requests ELSE 0 END AS Filled_rate
,CASE WHEN total_impressions>0 then total_revenue/(total_impressions::float/1000) ELSE 0 END Gross_eCPM
,CASE WHEN total_impressions>0 then total_net_revenue/(total_impressions::float/1000) ELSE 0 END Pub_net_eCPM
,CASE WHEN total_revenue>0 then ((total_revenue-total_net_revenue)::float/total_revenue) ELSE 0 END Verve_margin
,total_revenue-total_net_revenue AS Verve_profit
,displayed_impressions
,undisplayed_impressions
,CASE WHEN displayed_impressions>0 then undisplayed_impressions::float/displayed_impressions ELSE 0 END undisplayed_rate
FROM adsops.daily_agg_verve_delivery_mtd
WHERE eastern_date_sk = $P{p_date_sk}---Remember to pass parameter here
ORDER BY national_impressions DESC NULLS LAST
LIMIT 20 ---get the top 20]]>
	</queryString>
