	<field name="Total Gross" class="java.lang.Double"/>
	<variable name="eCPC" class="java.lang.Double">
		<variableExpression><![CDATA[( $F{Total Clicks}==0 ? null : $F{Total Gross}/$F{Total Clicks} )]]></variableExpression>
	</variable>
	<variable name="eCPM" class="java.lang.Double">
		<variableExpression><![CDATA[( $F{Total Verve Ad Filled}==0 ? null : $F{Total Gross}/($F{Total Verve Ad Filled}/1000) )]]></variableExpression>
	</variable>
	<variable name="CTR" class="java.lang.Double">
		<variableExpression><![CDATA[( $F{Total Verve Ad Filled}==0 ? null : $F{Total Clicks}/$F{Total Verve Ad Filled} )]]></variableExpression>
	</variable>
