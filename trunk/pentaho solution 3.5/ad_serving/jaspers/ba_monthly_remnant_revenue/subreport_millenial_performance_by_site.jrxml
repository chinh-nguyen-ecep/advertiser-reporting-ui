partner_name as site ,
requests,
gross_revenue,
p_net_revenue,
v_net_revenue,
ads_served as paid_impressions,
(case clicks when -1 then 0 else clicks end) as clicks,
net_revenue as net_revenue,
dt_lastchange,
partner_id,
parent1_name,
parent1_id,
portal_name,
portal_id,
partner_keyword,
portal_keyword,
fullfilled_code_count,
fullfilled_code_y
FROM billing.ba_monthly_mm_performance
WHERE calendar_year_month=$P{calendar_year_month}
AND is_active=true
ORDER BY site]]>
