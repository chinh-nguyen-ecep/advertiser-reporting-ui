  <Cube name="daily_event_maps" cache="true" enabled="true">
    <Table name="daily_event_maps_olap" schema="evttracker">
    </Table>
	<DimensionUsage name="YearMonth" source="YearMonth" foreignKey="eastern_date_sk"/>
    <Dimension type="StandardDimension" highCardinality="false" name="Id">
      <Hierarchy hasAll="true">
        <Level name="Id" column="id" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" highCardinality="false" name="Latitude">
      <Hierarchy hasAll="true">
