<Schema name="Event tracker">
    <Dimension name="YearMonth">
      <Hierarchy hasAll="true" primaryKey="date_sk">
		<Table name="date_dim" schema="refer"></Table>
		<Level name="Calendar year month" ordinalColumn="month_since_2005" column="calendar_year_month" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"></Level>
		<Level name="Full date" column="full_date" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never"></Level>
      </Hierarchy>
    </Dimension>
	
  <Cube name="daily_trane_stats" cache="true" enabled="true">
    <Table name="daily_trane_stats" schema="evttracker">
    </Table>
	<DimensionUsage name="YearMonth" source="YearMonth" foreignKey="eastern_date_sk"/>
    <Dimension type="StandardDimension" highCardinality="false" name="Event Name">
      <Hierarchy hasAll="true">
        <Level name="Event Name" column="event_name" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Measure name="Event count" column="event_count" aggregator="sum" visible="true">
    </Measure>
  </Cube>
  <Cube name="daily_event_stats" cache="true" enabled="true">
    <Table name="daily_event_stats" schema="evttracker">
    </Table>
	<DimensionUsage name="YearMonth" source="YearMonth" foreignKey="eastern_date_sk"/>
    <Dimension type="StandardDimension" highCardinality="false" name="Flight ID">
      <Hierarchy hasAll="true" allMemberName="All Flight ID">
        <Level name="Flight ID" column="flight_id" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" highCardinality="false" name="Creative ID">
      <Hierarchy hasAll="true" allMemberName="All Creative ID">
        <Level name="Creative ID" column="creative_id" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" highCardinality="false" name="Event Name">
      <Hierarchy hasAll="true">
        <Level name="Event Name" column="event_name" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Measure name="Event count" column="event_count" aggregator="sum" visible="true">
    </Measure>
  </Cube>
