	<queryString>
		<![CDATA[SELECT calendar_year_month AS "Month"
     , network_id AS "VLMO Agency ID"
     , network_title AS "VLMO Agency Name"
     , publisher_id AS "Account ID"
     , SUM(imps)::bigint AS "Delivered Imps"
     , SUM(total_delivered_imp_upto_month)::bigint AS "Total Delivered Imps Upto Month"
     , CASE WHEN SUM(remaining_imps)::bigint<0 THEN 0 ELSE SUM(remaining_imps)::bigint END AS "Remaining Imps"
     , SUM(clicks)::bigint AS "Clicks"
     , SUM(billable_imps)::bigint AS "Billable Imps"
     , billable_rate::float AS "Billable Rate"
     , (SUM(billable_imps)::double precision / 1000) * billable_rate AS "Gross Revenue"
FROM billing.ba_monthly_vlmo_revenue_v2
WHERE calendar_year_month = $P{calendar_year_month}::text
GROUP BY calendar_year_month
       , network_id
       , network_title
       , publisher_id
       , billable_rate
ORDER BY publisher_id]]>
	</queryString>
