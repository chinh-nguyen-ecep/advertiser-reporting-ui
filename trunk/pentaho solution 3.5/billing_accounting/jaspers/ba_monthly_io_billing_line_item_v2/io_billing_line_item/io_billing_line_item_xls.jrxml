	<queryString>
		<![CDATA[select
a.io_orders_id
,a.sfdc_opportunity_id
,a.organization_id
,o.adm_order_ids ----new column
,a.io_date
,a.coract_name as contract_name
,a.agency_name
,a.created_at
,a.updated_at
,a.revenue_goal
,a.comments_count
,a.quantity_goal
,a.agency_fee
,b.billing_line_items_id  as IO_line_item_id
,f.adm_flight_ids ----new column
,b.name_current as IO_lineitem_name
,b.created_at as IO_line_items_created_at
,b.updated_at as IO_line_items_updated_at
,b.flights_count as IO_line_items_flight_count
,b.quantity_current as IO_line_items_quantity
,b.io_date as IO_line_items_io_date
,b.external_ref as IO_line_items_external_ref
,b.comments_count as IO_line_items_comments_count
,b.budget_current as IO_line_items_budget
,b.start_date_current as IO_line_items_start_date
,b.end_date_current as IO_line_items_end_date
from refer.adm_dim_IO_orders a
inner join refer.adm_dim_io_lineitems b
on(b.dt_expire='9999-12-31' and b.insertion_order_id = a.io_orders_id)
--join to get adm_order_id list
left join (
select insertion_order_id, array_to_string(vlsm.array_accum(order_id),',') as ADM_Order_ids
from(
select insertion_order_id,order_id from refer.adm_dim_orders
where dt_expire = '9999-12-31'
and insertion_order_id <>-100
)a group by insertion_order_id
)o
on(a.io_orders_id = o.insertion_order_id)
--join to get adm flight ids list
left join(
select billing_line_item_id, array_to_string(vlsm.array_accum(flight_id),',')as ADM_Flight_ids
from(
select billing_line_item_id,flight_id from refer.adm_dim_flights
where dt_expire = '9999-12-31'
and network_id = 2
and billing_line_item_id <>-100
)a
group by billing_line_item_id
)f
on(b.billing_line_items_id = f.billing_line_item_id)
where
a.dt_expire = '9999-12-31'
order by io_orders_id;]]>
	</queryString>
