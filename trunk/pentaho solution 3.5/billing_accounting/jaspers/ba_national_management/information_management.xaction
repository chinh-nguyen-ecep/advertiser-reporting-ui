      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>JavaScript</action-type>
        <action-inputs> 
          <query_result type="result-set"/>  
          <query_result_history type="result-set"/> 
        </action-inputs>
        <action-outputs> 
          <displayName type="string"/>  
          <io_orders_id type="string"/>  
          <historyJson type="string"/>  
          <contact_name type="string"/>  
          <campaign_id type="string"/>  
          <created_by type="string"/>  
          <billing_contact type="string"/>  
          <created_at type="string"/>  
          <comment type="string"/>  
          <updated_at type="string"/>  
          <updated_by type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[var detailTable="";
var historyJson="";
var displayName=query_result.getValueAt(0,3);
var io_orders_id=query_result.getValueAt(0,1);
var contact_name=query_result.getValueAt(0,2);
var campaign_id=query_result.getValueAt(0,4);
var billing_contact=query_result.getValueAt(0,5);
var created_by=query_result.getValueAt(0,6);
var updated_by=query_result.getValueAt(0,8);
var created_at=query_result.getValueAt(0,7);
var updated_at=query_result.getValueAt(0,9);
var comment=query_result.getValueAt(0,10);

var meta = query_result_history.getMetaData(); 
var colHeaders = meta.getColumnHeaders(); 
historyJson='[';
	//process data item
	for(var i=0;i<query_result_history.getRowCount();i++){
		var item='';
		if(i>0){
			item+=',';
		}
		item+='{';
			for(var j=0;j<query_result_history.getColumnCount();j++){ 		
				var value=query_result_history.getValueAt(i,j);
				var columnValue=colHeaders[0][j];
				if(j>0){
					item+=',';
				}
				item+='"'+columnValue+'":"'+value+'"';
			}
		item+='}';	

	historyJson+=item;
	}
	//end

	historyJson+=']';]]></script> 
        </component-definition> 
      </action-definition>
  
      <action-definition> 
        <component-name>TemplateComponent</component-name>
        <action-type>Message Template</action-type>
        <action-inputs> 
