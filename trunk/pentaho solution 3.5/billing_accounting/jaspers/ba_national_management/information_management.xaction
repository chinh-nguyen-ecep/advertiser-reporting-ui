          <p_io_orders_id type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result_history"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[SELECT sk ,io_orders_id, campaign_id, billing_contact, created_by, created_at, 
       updated_by, updated_at, comment, dt_effective, dt_expire, is_active
FROM billing.ba_national_dim_io
WHERE io_orders_id ={p_io_orders_id}
ORDER BY sk DESC;]]></query>  
