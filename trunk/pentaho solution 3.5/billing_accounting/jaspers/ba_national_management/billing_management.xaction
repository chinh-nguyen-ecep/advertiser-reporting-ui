    <actions> 
      <condition><![CDATA[actions=='loadListIoOrders']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <p_month_since_2005 type="string"/>  
          <term type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[SELECT io_orders_id, contract_name 
FROM billing.ba_monthly_national_worksheet 
WHERE is_active = true and month_since_2005 = {p_month_since_2005}
AND (UPPER(io_orders_id||' | '||contract_name) LIKE UPPER('%{term}%') OR ''='{term}')
GROUP BY io_orders_id, contract_name 
ORDER BY io_orders_id;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadListIoLineItems']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <p_month_since_2005 type="string"/>  
          <p_io_orders type="string"/>  
          <limit type="string"/>  
          <page type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[select io_line_item_id, io_line_item_name 
from billing.ba_monthly_national_worksheet 
where is_active = true and month_since_2005 = {p_month_since_2005} 
and (io_orders_id IN ({p_io_orders}) OR '0'='{p_io_orders}') 
group by io_line_item_id, io_line_item_name 
order by io_line_item_id
OFFSET ({page}-1)*{limit} LIMIT {limit};]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadListMonths']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[select month_since_2005, calendar_year_month 
from billing.ba_monthly_national_worksheet 
where is_active = true
group by month_since_2005, calendar_year_month
order by month_since_2005 DESC
;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadBillingDetailTable']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <p_io_orders type="string"/>  
          <p_month_since_2005 type="string"/>  
          <p_io_line_items type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <live><![CDATA[true]]></live>  
          <query><![CDATA[select a.calendar_year_month
, a.month_since_2005
, a.advertiser_io as io_number
, a.updated_at::date as io_revision_date
, a.io_orders_id
, a.io_line_item_id
, a.io_line_item_start_date::date
, a.io_line_item_end_date::date
, a.organization_name as advertiser
, COALESCE(b.campaign_id, '') as campaign_id
, a.contract_name as campaign_name
, a.agency_name as agency
, COALESCE(b.billing_contact, '') as billing_contact
, (a.io_orders_id ||'-'|| a.io_line_item_id) as combined_ids
, a.io_line_item_name as placement_group
, a.io_line_item_external_ref as placement_id
, a.io_line_item_metric as rate_type
, a.io_line_item_rate as rate
, a.io_line_item_quantity as planned_units
, a.io_line_item_budget as contracted_budget
--,vendor_names as vendors
, a.dfp_impressions as dfp_delivered_imps
, a.dfp_clicks as dfp_delivered_clicks
, a.dfa_impressions as dfa_delivered_imps
, a.dfa_clicks as dfa_delivered_clicks
, a.adjusted_units --the user will update this field via UI
, a.delivered_units
, a.remaining_units
, a.billable_units
, ROUND(a.invoice_amount::NUMERIC, 2) as invoice_amount
, ROUND(a.total_amount_invoiced_upto_month::NUMERIC, 2) as total_amount_invoiced_upto_month
, a.remaining_budget
, CASE WHEN b.io_orders_id IS NULL THEN 'add' ELSE 'edit' END AS information_control
, CASE WHEN a.adjusted_units IS NULL THEN 'add' ELSE 'edit' END AS adjusted_units_control
from  billing.ba_monthly_national_worksheet a
left join billing.ba_national_dim_io b ON (b.is_active = true AND b.io_orders_id = a.io_orders_id AND b.io_line_item_id = a.io_line_item_id)
where month_since_2005 = {p_month_since_2005}
AND (a.io_orders_id IN ({p_io_orders}) OR '0'='{p_io_orders}')
AND (a.io_line_item_id IN ({p_io_line_items}) OR '0'='{p_io_line_items}')
AND a.is_active=true
order by advertiser, combined_ids;]]></query> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadBillingSummary']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <p_io_orders type="string"/>  
