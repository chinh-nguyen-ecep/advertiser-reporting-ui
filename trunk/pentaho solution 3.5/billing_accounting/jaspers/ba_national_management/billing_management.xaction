        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[select a.calendar_year_month
, a.month_since_2005
, a.advertiser_io as io_number
, a.io_orders_id
, a.updated_at::date as io_revision_date
, a.organization_name as advertiser
, a.contract_name as campaign_name
, a.agency_name as agency
, SUM(a.delivered_units) as delivered_units
, SUM(ROUND(a.invoice_amount::NUMERIC, 2)) as invoice_amount
, SUM(ROUND(a.total_amount_invoiced_upto_month::NUMERIC, 2)) as total_amount_invoiced_upto_month
from  billing.ba_monthly_national_worksheet a
where month_since_2005 = 113
AND (a.io_orders_id IN ({p_io_orders}) OR '0'='{p_io_orders}')
AND (a.io_line_item_id IN ({p_io_line_items}) OR '0'='{p_io_line_items}')
AND a.is_active=true
group by a.calendar_year_month
, a.month_since_2005
, a.advertiser_io
, a.io_orders_id
, a.updated_at::date
, a.organization_name
, a.contract_name
, a.agency_name
order by advertiser, a.io_orders_id;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
