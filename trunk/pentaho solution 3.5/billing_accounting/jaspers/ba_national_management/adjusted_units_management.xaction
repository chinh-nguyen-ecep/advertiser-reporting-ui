        <component-definition>
          <query><![CDATA[SELECT sk, io_orders_id, io_line_item_id, month_since_2005, adjusted_units, created_by, created_at, updated_by, 
       updated_at, comment, is_active
FROM billing.ba_national_dim_adjustment
