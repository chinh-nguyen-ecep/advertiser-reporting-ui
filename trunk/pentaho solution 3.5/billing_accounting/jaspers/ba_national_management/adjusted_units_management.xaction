    <actions> 
      <condition><![CDATA[actions=='loadListAjustedUnit']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <p_month_sk type="string"/>  
          <p_io_line_item_name type="string"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[SELECT sk, io_orders_id, io_line_item_id, combine_ids, io_line_item_name, 
       month_since_2005, adjusted_units, created_by, created_at, updated_by, 
       updated_at, comment, calendar_year_month
  FROM billing.vw_ba_national_dim_adjustment
WHERE (month_since_2005={p_month_sk} OR 0={p_month_sk})
AND ( UPPER(io_line_item_name) LIKE UPPER('%{p_io_line_item_name}%') OR ''='{p_io_line_item_name}')
ORDER BY calendar_year_month, combine_ids
;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadListMonths']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[SELECT a.month_since_2005,b.calendar_year_month
FROM billing.vw_ba_national_dim_adjustment a 
LEFT JOIN refer.month_dim b ON (a.month_since_2005=b.month_sk) 
GROUP BY a.month_since_2005,b.calendar_year_month 
ORDER BY a.month_since_2005 DESC;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadListMonthsForAdd']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/> 
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <live><![CDATA[true]]></live>  
          <query><![CDATA[SELECT a.month_since_2005 || ';' || b.full_date || ';' || c.full_date as month_id,  a.calendar_year_month
FROM (
SELECT a.month_sk as month_since_2005, a.calendar_year_month, date_sk_start, date_sk_end
FROM refer.month_dim a,
(SELECT MAX(month_since_2005) AS min_month_sk, MAX(month_since_2005) + 3 AS max_month_sk
FROM billing.ba_monthly_national_worksheet 
WHERE is_active = true) b
WHERE a.month_sk BETWEEN min_month_sk AND max_month_sk
) a
JOIN refer.date_dim b on b.date_sk = date_sk_start
JOIN refer.date_dim c on c.date_sk = date_sk_end
ORDER BY month_id]]></query> 
        </component-definition> 
      </action-definition>
 
    </actions>
  
    <actions> 
      <condition><![CDATA[actions=='loadListCombinedIDs']]></condition>  
      <action-definition> 
        <component-name>SQLLookupRule</component-name>
        <action-type>Relational</action-type>
        <action-inputs> 
          <jndi type="string" mapping="connectionName"/>  
          <limit type="string"/>  
          <page type="string"/>  
          <term type="string"/>  
          <p_start_date type="string"/>
          <p_end_date type="string"/>
          <p_month_sk type="string"/>
        </action-inputs>
        <action-outputs> 
          <query-result type="result-set" mapping="query_result"/> 
        </action-outputs>
        <component-definition> 
          <query><![CDATA[SELECT combined_ids, displayed_name
FROM (
 SELECT a.io_orders_id, b.billing_line_items_id AS io_line_item_id, (a.io_orders_id || '-'::text) || b.billing_line_items_id AS combined_ids, b.name_current AS io_line_item_name, (a.io_orders_id || '-'::text) || b.billing_line_items_id || ' | ' || b.name_current AS displayed_name
   FROM refer.adm_dim_io_orders a
   JOIN refer.adm_dim_io_lineitems b ON b.dt_expire = '9999-12-31' AND b.start_date_current::DATE <= '{p_end_date}'::DATE AND b.end_date_current::DATE >= '{p_start_date}'::DATE AND b.insertion_order_id = a.io_orders_id
  WHERE a.dt_expire = '9999-12-31'
  AND NOT EXISTS ( SELECT 1
           FROM billing.ba_national_dim_adjustment c
          WHERE c.is_active = true AND c.io_orders_id = a.io_orders_id AND c.io_line_item_id = b.billing_line_items_id AND c.month_since_2005 = {p_month_sk})
) a 
WHERE ('' = '{term}' OR UPPER(displayed_name) LIKE UPPER('%{term}%'))
ORDER BY combined_ids
OFFSET ({page}-1)*{limit} LIMIT {limit}
;]]></query>  
          <live><![CDATA[true]]></live> 
        </component-definition> 
      </action-definition>
 
