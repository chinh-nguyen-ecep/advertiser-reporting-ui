      <action-definition>
        <component-name>JavascriptRule</component-name>
        <action-type>JavaScript</action-type>
        <action-inputs>
          <query_result type="result-set"/>
          <query_result_history type="result-set"/>
        </action-inputs>
        <action-outputs>
          <combine_ids type="string"/>
          <io_line_item_name type="string"/>
          <month_since_2005 type="string"/>
          <adjusted_units type="string"/>
          <comment type="string"/>
          <historyJson type="string"/>
          <calendar_year_month type="string"/>
        </action-outputs>
        <component-definition>
          <script><![CDATA[var detailTable="";
var historyJson="";
var io_orders_id=query_result.getValueAt(0,1);
var io_line_item_id=query_result.getValueAt(0,2);
var combine_ids=query_result.getValueAt(0,3);
var io_line_item_name=query_result.getValueAt(0,4);
var month_since_2005=query_result.getValueAt(0,5);
var adjusted_units=query_result.getValueAt(0,6);
var created_by=query_result.getValueAt(0,7);
var updated_by=query_result.getValueAt(0,8);
var created_at=query_result.getValueAt(0,9);
var updated_at=query_result.getValueAt(0,10);
var comment=query_result.getValueAt(0,11);
var calendar_year_month=query_result.getValueAt(0,12);

var meta = query_result_history.getMetaData(); 
var colHeaders = meta.getColumnHeaders(); 
historyJson='[';
	//process data item
	for(var i=0;i<query_result_history.getRowCount();i++){
		var item='';
		if(i>0){
			item+=',';
		}
		item+='{';
			for(var j=0;j<query_result_history.getColumnCount();j++){ 		
				var value=query_result_history.getValueAt(i,j);
				var columnValue=colHeaders[0][j];
				if(j>0){
					item+=',';
				}
				item+='"'+columnValue+'":"'+value+'"';
			}
		item+='}';	

	historyJson+=item;
	}
	//end

	historyJson+=']';]]></script>
        </component-definition>
      </action-definition>

