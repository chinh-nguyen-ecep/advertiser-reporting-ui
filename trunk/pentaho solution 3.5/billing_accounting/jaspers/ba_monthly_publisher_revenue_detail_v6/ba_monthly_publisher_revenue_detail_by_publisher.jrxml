	<queryString>
		<![CDATA[SELECT
publisher_id
,publisher_name
,partner_id
,partner_name
,network_id
,(CASE WHEN network_id=2 THEN 'National' WHEN network_id=4 THEN 'VLM' WHEN network_id=3 THEN 'Remnant' WHEN network_id=6 THEN 'VPC' ELSE 'N/A' END) as network_name
,partner_revenue_share as publisher_revenue_share
,SUM(gross_revenue) as gross_revenue
,SUM(api_revenue) as net_revenue
,SUM(delivered_units) as impressions
,SUM(billable_revenue) as billable_revenue
,SUM(adjusted_billable_revenue) as adjusted_billable_revenue
FROM billing.ba_monthly_publisher_revenue_v6
WHERE is_active=true AND calendar_year_month=$P{calendar_year_month} AND partner_revenue_share>0 AND round(partner_revenue_share::numeric, 2)<1
GROUP BY publisher_id
,publisher_name
,partner_id
,partner_name
,network_id
,partner_revenue_share
ORDER BY publisher_name,partner_name]]>
	</queryString>
