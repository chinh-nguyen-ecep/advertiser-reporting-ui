	<queryString>
		<![CDATA[SELECT
'abc' as publisher_name
,partner_id
,adm_order_id
,flight_id
,flight_metric,
booked_cpm as booked_cpm,
verve_cpm as verve_cpm,
booked_impressions as booked_impressions
,(case when sum(gross_revenue)>0 then sum(v_net_revenue)/sum(gross_revenue) else 0 end) as verve_share,
sum(delivered_imps) as delivered_imps,
sum(partner_remaining_imps) as remaining_imps,
sum(partner_billable_imps) as billable_imps,
sum(gross_revenue) as gross_revenue,
sum(p_net_revenue) as p_net_revenue,
sum(v_net_revenue) as v_net_revenue
FROM adm.ba_monthly_national_revenue a
--LEFT JOIN refer.adm_publisher_dim b ON b.publisher_id=a.publisher_id AND dt_expire='9999-12-31'
WHERE calendar_year_month=$P{calendar_year_month}::text
AND delivered_imps>0
AND is_active=true
GROUP BY publisher_name,partner_id,adm_order_id,flight_id,flight_metric,booked_cpm,verve_cpm,booked_impressions
ORDER BY publisher_name,partner_id,adm_order_id,flight_id]]>
	</queryString>
