	<parameter name="p_network_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT a.* FROM (
	SELECT
		a.publisher_id  as publisher_id,
		b.company_name_current  as publisher,
		a.partner_id,
		sum(a.impressions) as paid_impressions,
		sum(a.gross_revenue) as gross_revenue,
		sum(a.p_net_revenue) as p_net_revenue,
		sum(a.v_net_revenue) as v_net_revenue
	FROM        adm.monthly_agg_adm_data_feed a
	INNER JOIN refer.adm_publisher_dim b ON b.dt_expire='9999-12-31' AND a.publisher_id=b.publisher_id
	INNER JOIN refer.partner_dim c ON c.dt_expire='9999-12-31' AND c.partner_id=a.partner_id
	WHERE       is_active = true
		AND calendar_year_month=$P{calendar_year_month}
		AND network_id=$P{p_network_id}::int
	GROUP BY
		a.publisher_id
		,publisher
		,a.partner_id
) a
ORDER BY publisher]]>
	</queryString>
