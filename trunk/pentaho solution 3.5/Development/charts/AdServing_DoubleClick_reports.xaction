        </action-outputs>
        <component-definition> 
          <script><![CDATA[v_chart_title=p_report_type;
if(v_user_choose_mode=='multiDimensions'){
	if(p_date_mode=='date'){
	queryOLAP='select Crossjoin({'+t_measures+'}, {'+t_site+'}) ON COLUMNS, {'+t_date_list+'} ON ROWS from [daily_agg_site_day] where {[Is active].[true]}';
	}else if(p_date_mode=='month'){
