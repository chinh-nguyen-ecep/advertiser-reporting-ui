      </action-inputs>
      <action-outputs> 
        <v_user_choose_mode type="string"/> 
      </action-outputs>
      <component-definition> 
        <script><![CDATA[var dimension;
dimension=typeof(p_Site);

if( dimension == 'string' ){
	v_user_choose_mode='onlyOneSite';
}else{
	v_user_choose_mode='multiDimensions';
}]]></script> 
      </component-definition> 
    </action-definition>
  
