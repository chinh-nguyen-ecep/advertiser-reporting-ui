	HTMLTable+='</tr>';
}else{
	HTMLTable+='<tr>';	
	HTMLTable+='<th>'+query_result_siteName.getValueAt(0,0)+'</th>';						
		HTMLTable+='<td>0</td>';
		HTMLTable+='<td>0</td>';
		HTMLTable+='<td>0</td>';
	HTMLTable+='</tr>';
};]]></script> 
          </component-definition> 
        </action-definition>
 
      </actions>
  
      <action-definition> 
        <component-name>JavascriptRule</component-name>
        <action-type>Create HTMLdata footer</action-type>
        <action-inputs> 
          <HTMLTable type="string"/> 
        </action-inputs>
        <action-outputs> 
          <HTMLTable type="string"/> 
        </action-outputs>
        <component-definition> 
          <script><![CDATA[HTMLTable+='</tbody></table>';]]></script> 
        </component-definition> 
      </action-definition>
  
      <actions> 
        <condition><![CDATA[chartSingleMode == 'true']]></condition>  
        <action-definition> 
