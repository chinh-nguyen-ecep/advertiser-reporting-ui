      <actions> 
        <condition><![CDATA[v_user_choose_mode=='multiDimensions']]></condition>  
        <actions loop-on="p_Site"> 
          <action-definition> 
            <component-name>SQLLookupRule</component-name>
            <action-type>GetSiteName</action-type>
            <action-inputs> 
              <p_Site type="string"/> 
            </action-inputs>
            <action-outputs> 
              <query-result type="result-set" mapping="query_result_siteName"/>  
              <dart_site_name type="string"/> 
            </action-outputs>
            <component-definition> 
              <jndi><![CDATA[dw3]]></jndi>  
              <query><![CDATA[SELECT dart_site_name
  FROM dbclk.monthly_agg_site WHERE dc_site_id={p_Site}]]></query>  
              <live><![CDATA[false]]></live> 
            </component-definition> 
          </action-definition>
  
          <action-definition> 
            <component-name>JavascriptRule</component-name>
            <action-type>CreateQuery</action-type>
            <action-inputs> 
              <p_Site type="string-list"/>  
              <p_start_date type="string"/>  
              <p_end_date type="string"/> 
            </action-inputs>
            <action-outputs> 
              <myQuery type="string"/> 
            </action-outputs>
            <component-definition> 
              <script><![CDATA[var myQuery;
myQuery='';

myQuery='select site_name,estimate_impressions,reserved_impressions,estimate_impressions-reserved_impressions as available_impressions from  dbclk.fn_get_site_forecast(';
myQuery+=p_Site;
myQuery+=',\'';
myQuery+=p_start_date;
myQuery+='\',\'';
myQuery+=p_end_date;
myQuery+='\') as myschema(site_name text,estimate_impressions integer, reserved_impressions integer)';]]></script> 
            </component-definition> 
          </action-definition>
  
          <action-definition> 
            <component-name>SQLLookupRule</component-name>
            <action-type>Relational</action-type>
            <action-inputs> 
              <p_end_date type="string"/>  
              <query type="string" mapping="myQuery"/> 
            </action-inputs>
            <action-resources/>
            <action-outputs> 
              <query-result type="result-set" mapping="query_result_forecast"/>  
              <site_name type="string"/>  
              <estimate_impressions type="integer"/>  
              <reserved_impressions type="integer"/>  
              <available_impressions type="integer"/> 
            </action-outputs>
            <component-definition> 
              <jndi><![CDATA[dw3]]></jndi>  
              <live><![CDATA[false]]></live> 
            </component-definition> 
          </action-definition>
  
          <action-definition> 
            <component-name>JavascriptRule</component-name>
            <action-type>Create HTMLdata row</action-type>
            <action-inputs> 
              <HTMLTable type="string"/>  
              <query_result_forecast type="result-set"/>  
              <p_Site type="string-list"/>  
              <query_result_siteName type="result-set"/> 
            </action-inputs>
            <action-outputs> 
              <HTMLTable type="string"/> 
            </action-outputs>
            <component-definition> 
              <script><![CDATA[var rowCount=query_result_forecast.getRowCount();

if(rowCount==1){
var site_name=query_result_forecast.getValueAt(0,0);
var estimate_impressions=query_result_forecast.getValueAt(0,1);
var reserved_impressions=query_result_forecast.getValueAt(0,2);
var available_impressions=query_result_forecast.getValueAt(0,3);

	HTMLTable+='<tr class="tr1">';
	
	HTMLTable+='<th>'+site_name+'</th>';
						
		HTMLTable+='<td>'+estimate_impressions+'</td>';
		HTMLTable+='<td>'+reserved_impressions+'</td>';
		HTMLTable+='<td>'+available_impressions+'</td>';

	HTMLTable+='</tr>';
}else{
	HTMLTable+='<tr>';	
	HTMLTable+='<th>'+query_result_siteName.getValueAt(0,0)+'</th>';						
		HTMLTable+='<td>0</td>';
		HTMLTable+='<td>0</td>';
		HTMLTable+='<td>0</td>';
	HTMLTable+='</tr>';
};]]></script> 
            </component-definition> 
          </action-definition>
 
        </actions>
 
      </actions>
  
