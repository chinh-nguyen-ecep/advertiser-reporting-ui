	<queryString>
		<![CDATA[select
o.order_id
,o.name_current as order_name
,o.publisher_id as booked_by_publisher_id
,p.company_name_current as booked_by_publisher_name
,f.rate_current as rate
,f.metric_current as type
,f.impressions as impressions_delivered
,f.clicks
,f.quantity as impressions_booked
,f.budget_current as budget
,f.updated_by as flight_last_update_by
,o.updated_by as Order_last_update_by
,f.updated_at as flight_last_update_date
,f.flight_id as ADM_Flight_ID
,f.dfp_line_item_id as DFP_flight_id
,f.name_current as flight_name
,f.start_date_current as ADM_flight_start_date
,f.end_date_current as ADM_flight_end_date
,f.created_at as ADM_created_at
,f.paused_at as ADM_paused_at
,case f.network_id when 1 then 'Local'
    when 4 then 'VLM' end Network
from refer.adm_dim_flights f
inner join refer.adm_dim_orders o
    on o.dt_expire = '9999-12-31' and f.order_id = o.order_id
inner join refer.adm_dim_publishers p
    on p.dt_expire = '9999-12-31' and p.publisher_id = o.publisher_id
where f.dt_expire = '9999-12-31'
and f.network_id in(1,4)
and f.cancelled_at is null
and f.start_date_current = $P{p_date}::date
and (f.impressions = 0 OR f.clicks = 0)
order by f.flight_id]]>
	</queryString>
