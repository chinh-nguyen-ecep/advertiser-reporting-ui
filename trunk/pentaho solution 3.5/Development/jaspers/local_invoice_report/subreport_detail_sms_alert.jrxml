	<queryString>
		<![CDATA[select
a.sms_description as "Description"
,a.sms_quantity as "Quantity"
,a.sms_rate as "Rate"
,(a.sms_quantity*a.sms_rate::double precision) as sms_fee
from  accounting.ba_sms_fee a
inner join

( select distinct inv_detail_id, account_partner_map_id
from accounting.ba_inv_Local_report_detail
where inv_mmyy = $P{p_month_year}

and status = 'ACTIVE'
)c
 on(c.account_partner_map_id = a.account_partner_map_id)
where
a.rpt_mmyy = $P{p_month_year}
and c.inv_detail_id = $P{p_inv_detail_id}
and a.account_partner_map_id = $P{account_partner_map_id}]]>
	</queryString>
