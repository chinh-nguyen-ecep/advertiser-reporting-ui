	<queryString>
		<![CDATA[SELECT	full_date,
	hour24_of_day,
	day_of_week,
	dma_id,
	dma_name,
	sum(event_count) as event_count
FROM evttracker.daily_event_dma_by_hour a
WHERE is_active=true
  AND full_date=$P{p_start_date}::date
  AND flight_id=$P{p_flight_id}
  AND event_name=$P{p_event_name}
GROUP BY full_date, hour24_of_day, day_of_week,dma_id, dma_name
ORDER BY full_date, hour24_of_day, day_of_week,dma_id, dma_name]]>
	</queryString>
