	<queryString>
		<![CDATA[SELECT	a.full_date,
	a.day_of_week,
	sum(a.event_count) as event_count
FROM evttracker.daily_event_dma a
WHERE  a.is_active=true
  AND a.event_name=$P{p_event_name}
  AND a.is_active=true
  AND a.flight_id=$P{p_flight_id}
  AND a.full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
GROUP BY a.full_date, a.day_of_week
ORDER BY a.full_date]]>
	</queryString>
