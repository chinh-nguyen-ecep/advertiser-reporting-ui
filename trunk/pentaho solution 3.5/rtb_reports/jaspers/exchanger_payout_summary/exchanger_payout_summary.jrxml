	<parameter name="p_exchangers" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT full_date as "Date"
     , exchanger as "Exchanger"
     , SUM(imp_cnt)::bigint as "Imp Cnt"
     , AVG(paid_price) as "Avg Paid Price"
     , AVG(bid_price) as "Avg Bid Price"
     , SUM(paid_amount) as "Paid Amount"
     , max(dt_lastchange) as dt_lastchange
FROM rtb.daily_agg_exchanger_payout
WHERE is_active = true
  AND full_date BETWEEN $P{p_start_date}::date AND $P{p_end_date}::date
  AND (exchanger IN (select unnest(string_to_array($P{p_exchangers}, ','))::text) OR '' = $P{p_exchangers})
GROUP BY full_date, exchanger
ORDER BY  full_date, exchanger;]]>
	</queryString>
