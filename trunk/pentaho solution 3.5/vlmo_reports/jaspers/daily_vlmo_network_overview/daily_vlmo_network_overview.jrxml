	<queryString>
		<![CDATA[SELECT	eastern_date_sk
	,full_date
	,(max(dt_lastchange)::timestamp with time zone AT TIME ZONE 'America/Los_Angeles')::timestamp with time zone AT TIME ZONE 'America/New_York'as dt_lastchange
	,partner_name AS "Site/app"
	,SUM(imp_cnt) AS "Impressions"
	,SUM(click_cnt) AS "Clicks"
	,SUM(cta_lp_cnt) AS "CTA LP"
	,SUM(cta_call_cnt) AS "CTA Call"
	,SUM(cta_save_coupon_cnt) AS "CTA Save Coupon"
	,SUM(cta_map_cnt) AS "CTA Map"
	,SUM(cta_face_cnt) AS "CTA Facebook"
	,SUM(cta_twit_cnt) AS "CTA Twitter"
	,CASE WHEN SUM(imp_cnt)>0 THEN SUM(click_cnt)/SUM(imp_cnt) ELSE 0 END AS "CTR"
	,CASE WHEN SUM(click_cnt)>0 THEN SUM(total_ctas)/SUM(click_cnt) ELSE 0 END AS "CTA"
	,SUM(total_ctas) AS "Total CTAs"
FROM vlmo_dw.daily_agg_network_overview
WHERE is_active = true
AND eastern_date_sk = $P{eastern_date_sk}::INTEGER
GROUP BY eastern_date_sk,full_date,partner_name
ORDER BY eastern_date_sk,partner_name]]>
	</queryString>
